.item {
    width: 120px;
    height: 120px;
    float: left;
    position: relative;
    margin: 0 10px 10px 0;
    border: 1px solid #ddd;
    background: url(/static/image/business/goods/addImg.png) no-repeat;
    background-size: 100% 100%;
}

.addImg {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 2;
    cursor: pointer;
}

.preview,
.preBlock {
    position: absolute;
    display: block;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
}

.delete {
    width: 18px;
    height: 18px;
    position: absolute;
    right: 0;
    top: 0;
    cursor: pointer;
    display: none;
    z-index: 3;
}

.preBlock img {
    display: block;
    width: 100%;
    height: 100%;
}

.upload_input {
    display: block;
    width: 0;
    height: 0;
    -webkit-opacity: 0.0;
    /* Netscape and Older than Firefox 0.9 */
    -moz-opacity: 0.0;
    /* Safari 1.x (pre WebKit!) 老式khtml内核的Safari浏览器*/
    -khtml-opacity: 0.0;
    /* IE9 + etc...modern browsers */
    opacity: .0;
    /* IE 4-9 */
    filter: alpha(opacity=0);
    /*This works in IE 8 & 9 too*/
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(Opacity=0)";
    /*IE4-IE9*/
    filter: progid:DXImageTransform.Microsoft.Alpha(Opacity=0);
}
.input-text[type="number"] {
    width: 100%;
}
.content_number {
    width: 100%;
    background: #fafafa;
    padding-bottom: 15px;
    display: none;
}
.content_number_list {
    margin-left: 18px;
    margin-right: 18px;
    padding-top: 15px;
    overflow: hidden;
}
.content_number_list img {
    width: 80px;
}
.content_number_list .content_margin_left {
    margin-left: 18px;
}
.content_number_list span {
    color: #999999;
    font-size: 12px;
}
.content_number_list .content_price_view {
    display: inline-block;
    width: 170px;
    margin-left: 10px;
    overflow: hidden;
    vertical-align: middle;
}
.content_number_list .content_number_view {
    display: inline-block;
}
.application_tab_li {
    display: block;
}
#kdf {
    display: none;
}
.over {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: 2;
    left: 0;
    top: 0;
    display: none;
}