body{
	background: #f2f2f2;
}
main{
	/*margin-top: 0.9rem;*/
}

.register_inp input{
	width:5.58rem;
	text-indent: 0.5rem;
	float: right;
}
.register_list{
	background: #fff;
	border-bottom: 0.01rem solid #f5f5f5;
}
.register_inp{
	padding-top: 0.3rem;
	padding-bottom: 0.3rem;
	margin-left: 0.3rem;
	margin-right: 0.3rem;
	overflow: hidden;
	position: relative;
}
.register_inp p{
	margin-top: 0.03rem;
	float: left;
}
.register_inp img{
	width:0.42rem;
	height:0.42rem;
	position: absolute;
	/*top:0.23rem;*/
	right:0.19rem;
}
.register_inp textarea{
	width: 100%;
}
.editaddress_margin{
	background: #f2f2f2;
}
.register_inps{
	padding-top: 0.3rem;
	padding-bottom: 0.3rem;
	margin-left: 0.3rem;
	margin-right: 0.3rem;
	overflow: hidden;
}
.register_inps img{
	width:0.28rem;
	height:0.28rem;
	vertical-align: middle;
	display: none;
}
.register_inps .address_icon{
	display: inline-block;
}
.register_inps span{
	margin-left: 0.2rem;
	vertical-align: middle;
	color:0.24rem;
	font-size: #000000;
}
.register_btn{
	width:6.9rem;
	height:0.88rem;
	margin:0 auto;
	background-image: linear-gradient(0deg, 
		#FFE500 0%, 
		#FFED50 100%), 
	linear-gradient(
		#FFE500, 
		#FFE500);
	line-height: 0.88rem;
	border-radius: 0.18rem;
	text-align: center;
	color:#000000;
	font-size: 0.32rem;
	font-weight: 600;
}