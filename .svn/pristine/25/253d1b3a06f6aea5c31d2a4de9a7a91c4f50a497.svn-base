.inputBoxContainer{
    width: 240px;
    height: 42px;
    margin: 0 auto;
    position: relative;
}
.inputBoxContainer .bogusInput{
    width: 100%;
    height: 100%;
    border: #c3c3c3 1px solid;
    border-radius: 7px;
    -moz-border-radius: 7px;
    -webkit-border-radius: 7px;
    overflow: hidden;
    position: absolute;
    z-index: 0;
}
.inputBoxContainer .realInput{
    width: 200%;
    height: 100%;
    position: absolute;
    top:0;
    left: 0;
    z-index: 1;
    filter:alpha(opacity=0);
    -moz-opacity:0;
    opacity:0;
    color:transparent;
    text-indent: -999rem;
    margin-left: -100%; 
}

.inputBoxContainer .bogusInput input{
    padding: 0;
    width: 16.3%;
    height: 100%;
    float:left;
    background-color: #fff;
    text-align: center;
    font-size: 20px;
    border: none;
    border-right: #C3C3C3 1px solid;
}
.inputBoxContainer .bogusInput input:last-child{
    border: none;
