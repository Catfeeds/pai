body{
    background: #fff;
}
.header_view{
    border-bottom: 0.01rem solid #f5f5f5;
}
.confirm_main{
    width:6.9rem;
    margin:0 auto;
    padding-top: 0.73rem;
}
.confirm_img{
    width:0.7rem;
    height:0.7rem;
    margin:0 auto;
}
.confirm_img img{
    width:100%;
    height:100%;
}
.confirm_main p{
    text-align: center;
    font-size: 0.34rem;
    color:#000000;
    font-weight: 600;
    margin-top: 0.3rem;
}
.confirm_main .confirm_text{
    color:#aaaaaa;
    font-size: 0.24rem;
    text-align: center;
    font-weight: 200;
    margin-top: 0.23rem;
}
.confirm_chakan{
    width:1.2rem;
    height:0.48rem;
    border:0.01rem solid #333333;
    border-radius: 0.09rem;
    margin:0 auto;
    margin-top: 0.55rem;
    color:#000000;
    font-size: 0.24rem;
    line-height: 0.48rem;
    text-align: center;
    font-weight: 600;
}
.confirm_chakanpingjia{
    margin-top:0.37rem ;
}
.confirm_btn{
    background-image: linear-gradient(90deg, #FFE500 0%, #FFED50 100%), linear-gradient(#FFE500, #FFE500);
    height:0.88rem;
    line-height: 0.88rem;
    text-align: center;
    color:#000000;
    font-weight: 600;
    font-size: 0.3rem;
    border-radius: 0.18rem;
    margin-top: 1.38rem;
}