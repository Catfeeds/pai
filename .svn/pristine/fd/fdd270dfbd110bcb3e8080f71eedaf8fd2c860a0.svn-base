.all_regiments_top {
    font-size: 0.24rem;
    font-family: PingFangSC-Light;
    font-weight: 300;
    color: rgba(170, 170, 170, 1);
    line-height: 0.88rem;
    margin-left: 0.31rem;
}

.regiment_detail {
    background-color: white;
    width: 100%;
    height: 1.6rem;
}

.regiment_detail img {
    width: 1.2rem;
    height: 1.2rem;
    background: #0C0C0C;
    margin-top: 0.2rem;
    margin-left: 0.3rem;
    margin-right: 0.2rem;
}

.regiment_detail_info {
    width: 5.5rem;
    margin-right: 0.3rem;
    margin-top: 0.28rem;
}

.pd_name {
    font-size: 0.26rem;
    font-family: PingFangSC-Regular;
    font-weight: 400;
    color: rgba(0, 0, 0, 1);
    line-height: 0.34rem;
    word-break: break-all;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
}
.pd_price{
    font-size:0.26rem;
    font-family:PingFangSC-Regular;
    font-weight:600;
    color:rgba(255,52,52,1);
    line-height:0.34rem;
    margin-top: 0.18rem;
}
.data-list{
    background-color: white;
}
.tuanzhong_img{
    width: 0.72rem;
    height: 0.72rem;
    border-radius: 50%;
    background-color: #0C0C0C;
    margin: 0.19rem 0.29rem 0.2rem 0.3rem;
}
.tuanzhong_info{
    width: 5.8rem;
    margin-right: 0.3rem;
}
.tuanzhong_name{
    font-size:0.28rem;
    font-family:PingFangSC-Medium;
    font-weight:600;
    color:rgba(0,0,0,1);
    line-height:0.3rem;
}
.tuanzhong_date{
    font-size:0.24rem;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(170,170,170,1);
    line-height:0.3rem;
}
.tuanzhong_data{
    font-size:0.24rem;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(120,120,120,1);
    line-height:0.3rem;
}
.go_more{
    width: 0.24rem;
    height: 0.24rem;
}