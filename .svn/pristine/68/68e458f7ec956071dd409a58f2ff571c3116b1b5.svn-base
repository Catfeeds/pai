@charset "utf-8";
/* 头部-底部主菜单 */	
.top{background-color:#de3232;width: 100%;overflow: hidden;height: 44px;position:fixed;top:0;left:0;z-index:100;}
.top .city {width: 44px;height: 44px;float: left;}
.top .top_title {color: #FFFFFF;font-size:18px;text-align: center;width: 100%;line-height: 1em;}
.top .rig_btn {height:40px;width:40px;float: right;}

.foot_nav1{height:63px;width: 100%; position:fixed; bottom:0; background-color:#f6f6f6;padding-bottom:2px;z-index:100;}
.foot_nav1 p{height:8px;background-color:#ebecee;width:100%;}
.foot_nav1 ul{border-top:2px solid #c0c1c3; }
.foot_nav1 li{float:left;width:25%;}
.foot_nav1 img {height: auto;width: 100%;}
.foot_nav1 span{height: auto;width: 100%; text-align:center; display:block;}
.copyright {width: 100%;text-align: center;}

/* 广告 */
.banner{background:#fbf8f3;width: 100%;}
.relative{position:relative;left:0;top:0;}
.scroll{max-width:640px;margin-right: auto;margin-left: auto;}
.scroll_box{overflow:hidden;visibility:hidden;position:relative;}
.scroll_wrap{overflow:hidden; position:relative;}
.scroll_wrap li{position:relative;display:block;width:100%;float:left;}
.scroll_wrap li a{display:block;margin:0 auto;position:relative;}
.scroll_wrap img{width: 100%;}
.scroll_position{position:absolute;left:45%;z-index:400px;bottom:8px;}
.scroll_position li{display:inline-block;width:10px;height:10px;border-radius:10px;background:#fff;}
.scroll_position li a{font-size:0;}
.scroll_position li.on{background-color:#ff621d;}
.scroll_position_bg{background:#000;position:absolute;bottom:12px;left:42%;padding:0 15px;z-index:380px;height:26px;border-radius:26px;}

/* 信息 */
.wrap{width:100%;}
.infor_list{width: 100%;}
.infor_list li {width: 100%;}
.infor_list .nav {background-color: #ffe1d5;width: 96%;padding-top: 0;padding-right: 1%;padding-bottom: 0;padding-left: 3%;float: left;}
.infor_list .nav .title img {height: 17px;width: 17px;}
.infor_list .nav .title {float: left;width: 47%;font-size: 16px;line-height: 2em;}
.infor_list .nav .time {float: left;width: 49%;text-align: right;font-size: 16px;line-height: 2em;}
.infor_list .content {float: left;width: 94%;line-height: 2em;padding-top: 5px;padding-right: 3%;padding-bottom: 5px;padding-left: 3%;}

/* 产品详情页 */
.bg{background-color: #e2e2e2;}
.product{height: 240px;position: relative;width: 100%;overflow: hidden;}
.product .product_img img {width: 100%;height: auto;}
.product .content {height: 75px;position: absolute;bottom: 0px;width: 100%;left: 0px;right: 0px;background-color: #000000;filter:alpha(opacity=50);-moz-opacity:0.5;-khtml-opacity: 0.5;opacity: 0.5;top: 165px;z-index: 10;}
.product .content .title {float: left;width: 80%;color: #FFFFFF;padding-left: 2%;}
.product .content .title p {font-size: 20px;line-height: 2em;}
.product .content .rig_btn {float: left;width: 18%;text-align: center;z-index: 11;}
.price_cont {width: 100%;background-color: #FFFFFF;}
.price_cont .price {font-size:18px;line-height: 2em;color: #ff621d;float: left;width: 48%;padding-left: 2%;}
.price_cont .price em {font-size: 16px;color: #999999;text-decoration: line-through;}
.price_cont .price_number {float: left;width: 48%;text-align: right;padding-right: 2%;font-size: 16px;line-height: 2.5em;}
.pay_cont {height:50px;width: 100%; position:fixed; bottom:0; background-color: #FFF;border-top: 1px solid #e5e5e5;}
.pay_cont .price {font-size:18px;line-height: 2.4em;color:red;float: left;width: 48%;padding-left: 2%;font-weight: bold;margin-top:5px;}
.pay_cont .price em {font-size: 16px;color: #999999;text-decoration: line-through;}
.pay_cont .price_number {float: left;width: 48%;padding-right: 2%;font-size: 16px;line-height: 2.5em;padding-top: 3px;}
.applyBtn{width:90px;height:28px;padding-bottom:2px;color:#fff;cursor:pointer;background-color: #e13c3c;border-radius:4px;font-family: "微软雅黑";	font-size:15px;line-height:28px;margin-top: 7px;margin-bottom: 7px;padding-right:0px;padding-left: 0px;display: block;float: right;text-align: center;}
.applyBtn:hover{color: #FFFFFF;	background-color: #ff6c70;}
.padding-b{padding-bottom: 70px;}


/* 支付弹框样式 */
.theme-gobuy,.theme-signin{font-size: 15px;}
.theme-popover{z-index:9999;position:fixed;bottom:0;left:0;width:100%;height:auto;border-radius:5px;border:1px solid #CCCCCC;background-color:#fff;display:none;box-shadow: 0 0 10px #666;margin-right: 0px;margin-left:0;margin-top:0;margin-bottom: 0px;min-width:320px;}
.theme-popover-mask{z-index: 9998;position:fixed;top:0;left:0;width:100%;height:100%;background:#000;opacity:0.4;filter:alpha(opacity=40);display:none}
.theme-poptit{border-bottom:1px solid #ddd;padding:12px;position: relative;height:18px;background-color: #f2f1f1;}
.theme-poptit .close{float:right;color:#FFFFFF;padding:5px;margin:-2px -5px -5px;text-shadow:0 1px 0 #ddd;display: block;background-color: #cbcbcb;font-family: simsun;font-size: 14px;line-height: 14px;font-weight: bold;}
.theme-poptit .close:hover{color:#444;}
.theme-poptit h3 {float: left;font-size: 14px;line-height: 25px;height: 25px;}
.theme-popbod{color:#444;height:auto;padding-top: 30px;padding-right: 15px;padding-bottom: 30px;padding-left: 15px;}
.theme-signin{text-align:left;font-size: 14px;padding: 0px;}
.theme-signin li{width: 100%;height: 50px;}
.theme-signin .left_in {float: left;width: 30%;line-height: 30px;text-align: right;margin-bottom: 5px;padding-top: 5px;}
.theme-signin .right_in {height: 30px;float: left;width: 70%;margin-bottom: 5px;}
.theme-signin .right_in input {line-height: 28px;color: #999999;padding-left: 10px;width: 70%;min-width:165px;}
.tj_Btn{width:85%;height:36px;padding-bottom:2px;cursor:pointer;background-color: #ff6c70;font-family: "微软雅黑";font-size: 18px;
line-height: 36px;margin-top: 7px;margin-bottom: 7px;text-align: center;display: block;color: #FFFFFF;margin-right: auto;margin-left: auto;}
.tj_Btn:hover{background-color: #e13c3c;color: #FFFFFF;}

