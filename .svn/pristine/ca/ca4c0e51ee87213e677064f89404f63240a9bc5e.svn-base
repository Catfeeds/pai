
.title{
    width: 100%;
    height: 1rem;
    background: #FFF;
    position:fixed;
    z-index: 20;
}
.title span.spans{
    text-align: center;
    font-size: 0;
    width: 3.6rem;
    border-radius:0.1rem;
    background: #FFF;
    position: absolute;
    left: 0;
    right: 0;
    margin:0 auto;
    height:0.56rem;
    margin-top:0.2rem;
    border-radius: 0.1rem;
    overflow: hidden;

}
.title span{
    display: inline-block;
    width:1.8rem;
    font-size:0.26rem;
    font-weight:500;
    line-height:0.56rem;
    background:rgba(242,242,242,1);
    color:#999999;
    /* margin-top: 0.2rem; */
}
.title span.active{
    color:rgba(255,255,255,1);
    background:linear-gradient(90deg,rgba(255,115,35,1),rgba(255,167,44,1));
}


.black{
    width: 100%;
    height: 0.1rem;
    background: #f5f5f5;
}
.main_content{
    background-color:#FFFFFF ;
    margin-top: 1rem;
    /*display: none;*/
}
.main_content li,.main_list li{
    width: 100%;
    height: 2.4rem;
    position: relative;
    box-sizing: border-box;
    padding:0.14rem 0.29rem  0.2rem 0.34rem;
    border-bottom: 0.01rem rgba(245,245,245,1)solid ;
}
.main_content li a {
    display: block;
}
.main_content li a:nth-child(2){
    background:rgba(242,242,242,1);
    position: absolute;
    top: 0.85rem;
}
.main_content li a .user_img{
    width: 0.56rem;
    height: 0.56rem;
    /*background: red;*/
    border-radius: 50%;
}
.main_content li a .user_name{
    font-size: 0.26rem;
    margin-left: 0.2rem;
    margin-right: 0.1rem;
    color:rgba(51,51,51,1);
    font-weight:bold;
}
.main_content li a .user_info{
    font-size: 0.24rem;
    font-weight:bold;
    color:rgba(51,51,51,1);
}
.main_content li a .pro_img{
    width: 1rem;
    height: 1rem;
    /*background: red;*/
    display: inline-block;
    vertical-align: middle;
}
.main_content li a .pro_info{
    width: 5.7rem;
    height: 1rem;
    display: inline-block;
    margin-left: 0.2rem;
    vertical-align: middle;
    position: relative;
}
.main_content li a .pro_info .pro_name{
    font-size:0.26rem;
    font-weight:bold;
    display: block;
    width: 5.19rem;
    color:rgba(102,102,102,1);
    line-height:0.3rem;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    position: absolute;
    top: 0.2rem;
}

.main_content li a .pro_info .pro_order{
    font-size:0.24rem;
    font-weight:400;
    color:rgba(170,170,170,1);
    line-height:0.32rem;
    position: absolute;
    top: 0.6rem;
}

.main_content .call_date{
    font-size:0.24rem;
    font-weight:400;
    color:rgba(179,179,179,1);
    line-height:0.32rem;
    position: absolute;
    bottom: 0.2rem;
    right: 0.31rem;
}


.main_list{
    background-color:#FFFFFF ;
    margin-top: 1rem;
    /*display: none;*/
}
.main_list li{
    height: 1.4rem;
}
.main_list li a {
    width: 5.75rem;
    float: left;
}
.main_list li a div .user_img{
    width: 0.56rem;
    height: 0.56rem;
    /*background: red;*/
    border-radius: 50%;
    margin-top: 0.24rem;
}
.main_list li .pro_img{
    width: 1rem;
    height: 1rem;
    /*background: red;*/
    float: right;
}
.main_list li a span{
    font-size:0.24rem;
    font-weight:400;
    text-align: center;
    vertical-align: middle;
    color:rgba(51,51,51,1);
}
.main_list li a span.user_name{
    font-size:0.26rem;
    font-weight:bold;
    margin-left: 0.1rem;
    display: inline-block;
    max-width: 1.5rem;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
}
.main_list li a span.pro_name{
    /* width: 1.89rem; */
    /* height: 0.3rem; */
    display: inline-block;
    font-size:0.26rem;
    font-weight:bold;
    line-height:0.3rem;
    /* overflow:hidden; */
    /* text-overflow:ellipsis; */
    /* white-space:nowrap */
}
.main_list .date_info{
    font-size:0.24rem;
    font-weight:400;
    color:rgba(179,179,179,1);
    line-height:0.32rem;
    margin-left: 0.6rem;
}
.swiper-container {
    position: fixed;
    width: 7.5rem;
    top: 1rem;
    left: 0;
    right: 0;
    bottom: 0;
}