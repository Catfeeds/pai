.continue_pop{
	width:7.5rem;
	position:fixed;
	top:0;
	left: 0;
	bottom:0;
	right:0;
	background: rgba(0,0,0,0.4);
	z-index:100;
	display: none;
}
.continue_con{
	width:5.4rem;
	background: #fff;
	margin:0 auto;
	margin-top: 2.53rem;
	overflow: hidden;
	padding: 0 0 0.5rem 0;
}
.continue_con_top{
	width:3.46rem;
	height:0.28rem;
	margin:0 auto;
	margin-top: 0.48rem;
}
.continue_con_top img{
	width:100%;
	height:100%;
}
.continue_con_code{
	width:2.8rem;
	height:2.8rem;
	overflow: hidden;
	margin:0 auto;
	margin-top: 0.47rem;
}
.continue_con_code img{
	width:100%;
	height:100%;
}
.continue_con_btn{
	width:3.84rem;
	margin:0 auto;
	margin-top:0.47rem;
}
.bc-btn {
    padding:0.1rem;
    text-align: center;
    color: #aaa;
    font-size: 0.24rem;
    margin: 0.2rem auto 0 auto;
}
.copy-btn {
    width: 1.88rem;
    height: 0.42rem;
    border-radius: 0.1rem;
    display: block;
    border: 1px solid #000;
    margin: 0.3rem auto 0 auto;
    text-align: center;
    line-height: 0.42rem;
    font-size: 0.28rem;
}
.continue_con_btn div{
	border:0.02rem solid #000000;
	border-radius: 0.09rem;
	padding:0.1rem;
	text-align: center;
	color: #000000;
	font-size: 0.24rem;
	font-weight: 600;
	margin: 0 auto;
}
.continue_con_list{
	width:5.4rem;
	margin-top:0.7rem ;
	padding-left: 0.3rem;
	padding-right: 0.3rem;
}
.continue_con_list_div{
	width:1.2rem;
}
.continue_con_list_div div{
	width:0.8rem;
	height:0.8rem;
	margin:0 auto;
	margin-bottom: 0.2rem;
}
.continue_con_list_div div img{
	width:100%;
	height:100%;
}
.continue_con_list_div p{
	text-align: center;
	font-size:0.24rem;
	color:#333333;
}
.details_fenxiang{
	width:7.5rem;
	position: fixed;
	top:0;
	left: 0;
	bottom:0;
	right:0;
	background: rgba(0,0,0,0.2);
	z-index: 3;
	display: none;
}
.details_fxcon{
	width:7.5rem;
	background: #fff;
	position: fixed;
	bottom:0;
	left: 0;
	z-index: 3;
}
.details_fxtit{
	text-align: center;
	padding-top: 0.25rem;
	padding-bottom: 0.25rem;
	color:#666666;
	font-size: 0.22rem;
}
.details_fxlist{
	width:7.5rem;
	background: #F2F2F2;
	padding-top: 0.5rem;
	padding-bottom: 0.5rem;
	/*padding-left: 0.3rem;*/
	padding-right: 0.3rem;
}
.details_fx_img{
	width:1.44rem;
	text-align: center;
	color:#999999;
	font-size: 0.22rem;
	padding-left: 0.8rem;

}
.details_fx_img img{
	display: inline-block;
	margin-bottom: 0.24rem;
	width:100%;
	height:100%;
}
.details_fx_cancel{
	height:0.88rem;
	background: #fff;
	color:#7E7E7E;
	font-size: 0.3rem;
	line-height: 0.88rem;
	text-align: center;
}


.vip-photo h3 {
	margin: 0 3rem 0 1.7rem;
	font-weight: bold;
	font-size: 0.28rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.vip-photo p {
	position: absolute;
	right: 0.3rem;
	top: 0;
	vertical-align: middle;
	background: url(../../image/core/jt.png) no-repeat center right;
	background-size: 0.3rem;
	padding: 0 0.4rem 0 0;
	font-size: 0.22rem;
	font-weight: bold;
}
.vip-photo p span {
	color: #aaa;
	font-weight: normal;
}
.vip-photo p img {
	width: 0.36rem;
	height: 0.36rem;
	border-radius: 100%;
	padding: 0 0.1rem;
}
.open-tj {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 999999;
	display: none;
}
.open-over {
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.4);
}
.open-cont {
	width:5.8rem;
	height:3.2rem;
	background:#fff;
	border-radius:0.08rem;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}
.open-cont span {
	display: block;
	position: absolute;
	width: 0.44rem;
	height: 0.44rem;
	background: url(../../image/core/close.png) no-repeat center;
	background-size: 100%;
	right: 0.57rem;
	top: 0.08rem;
}
.open-cont h3 {
	height: 0.68rem;
	border-bottom: 1px solid #f5f5f5;
	margin: 0 0.9rem 0.26rem 0.9rem;
	text-align: center;
	line-height: 0.68rem;
	color: #666;
	font-weight: bold;
	font-size: 0.24rem;
}
.open-cont img {
	width: 0.68rem;
	height: 0.68rem;
	display: block;
	margin: 0 auto 0.16rem auto;
	border-radius: 100%;
}
.open-cont p {
	color: #333;
	font-size: 0.26rem;
	font-weight: bold;
	text-align: center;
	margin: 0 0 0.34rem 0;
}
.open-cont div {
	margin: 0 1.14rem;
	color: #666;
	font-size: 0.26rem;
}