.header_nav{
	background: transparent;
	border:none;
}
.vip_top{
	width:7.5rem;
	background:url("../../image/core/top_img@2x.png") no-repeat center center;
	background-size: 100% 100%;
	padding-top: 1rem;
	height: 5.7rem;
}
.header_tit{
	color:#fff;
}
.header_tit img.yaya{
	width: 2.16rem;
	height: 0.8rem;
	position: absolute;
	left:2.1rem;
	top: 0.15rem;
}
.header_tit img.promo{
	width: 1.9rem;
	height: 0.54rem;
	position: absolute;
	left: 5.6rem;
	top: 0.2rem;
	/*margin-left: 0.76rem;*/
}
.vip_head_portrait{
	width:1.36rem;
	height:1.36rem;
	border-radius: 50%;
	background:url("../../image/core/pic_headring@2x.png") no-repeat center center;
	background-size: 100% 100%;
	z-index: 2;
	margin: 0 auto;
	overflow: hidden;
	/*background: transparent;*/
}

.header_info{
	width: 100%;
	margin-top: 0.24rem;
}
.my_info{
margin-left: 0.58rem;
}
.my_info_detail{
margin-left: 0.3rem;
}
.my_info_detail p{
	font-size:0.28rem;
	font-family:PingFangSC-Medium;
	font-weight:500;
	color:rgba(255,255,255,1);
}
.my_info_detail .my_info_level{
	width: 1.16rem;
	height: 0.4rem;
}
.todetail{
	width: 0.28rem;
	height: 0.28rem;
	margin-left: 0.56rem;
	margin-top:-0.5rem ;
}
.my_info .tx,.tui_info .tx{
	width: 1rem;
	height: 1rem;
	border-radius: 100%;
	border: 2px solid #fff;
}
.tui_info{
	margin-right: 0.46rem;
}
.tui_info .tx{
	border: none;
}

.tui_info_detail .tui_info_name{
	font-size:0.26rem;
	font-family:PingFangSC-Medium;
	font-weight:500;
	color:rgba(120,120,120,1);
}

.tui_info_detail .tui_info_tip{
	width: 1.08rem;
	height: 0.2rem;
}
.tui_info_detail .todetail{
	margin-left: 0.1rem;
	margin-top: -0.7rem;
}
















