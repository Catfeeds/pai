/* 公告s */
.details-act {
	padding: 0 0.3rem 0 0.9rem;
	height: 0.6rem;
	background:#eb6266;
	position: relative;
	overflow: hidden;
	font-size: 0.22rem;
}
.details-act span {
	position: absolute;
	display: block;
	width: 0.8rem;
	height: 0.6rem;
	left: 0;
	top: 0.15rem;
	background: url(../../image/popularitygoods/gg.png) no-repeat #eb6266 top right;
	background-size: 0.5rem;
	z-index: 9;
}
.details-act div {
	height: 100%;
	line-height: 0.6rem;
	background: #eb6266;
}
.details-act a {
	color: #fff;
	margin: 0 0.8rem;
	font-size: 0.22rem;
}
.dowebok:after {
	content: '';
	display: table;
	clear: both;
}
.dowebok p {
	display: inline;
	color: #fff;
	font-size: 0.22rem;
	margin: 0 0.8rem;
	line-height: 0.6rem;
}
.details-act small {
	position: absolute;
	display: block;
	width: 0.3rem;
	height: 0.6rem;
	right: 0;
	z-index: 9;
	background: #eb6266;
}
