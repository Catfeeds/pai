body{
    background:#fff;
}
.cancel{
    margin-top: 0.68rem;
}
.cancel_money{
    width:0.7rem;
    height:0.7rem;
    margin:0 auto;
}
.cancel_money img{
    width:100%;
    height:100%;
}
.cancel p{
    text-align: center;
    color:#000000;
    font-size: 0.34rem;
    font-weight: 600;
    margin-top: 0.38rem;
}
.cancel span{
    display:block;
    text-align: center;
    color:#aaaaaa;
    font-size: 0.24rem;
    margin-top: 0.24rem;
}
.resubmit_btn{
    width:6.9rem;
    height:0.88rem;
    background:linear-gradient(0deg,rgba(255,229,0,1),rgba(255,237,80,1));
    box-shadow:0px 18px 21px 0px rgba(255,229,0,0.15);
    border-radius:18px;
    margin:0 auto;
    margin-top: 1.38rem;
    color:#000000;
    font-size: 0.3rem;
    font-weight: 600;
    text-align: center;
    line-height: 0.88rem;
}