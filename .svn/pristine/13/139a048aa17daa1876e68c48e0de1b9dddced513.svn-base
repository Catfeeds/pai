.participate_tab{
    width: 100%;
    height:0.88rem;
    background: #fff;
    position: fixed;
    top:2.48rem;
    left: 0;
    z-index: 3;
}
.participate_tab_list{
    width:33%;
    text-align: center;
}
.participate_tab_list span{
    font-size: 0.24rem;
    color:#AAAAAA;
    width: 90%;
    display: block;
    height: 0.48rem;
    line-height: 0.48rem;
    margin: 0.2rem 0 0 0;
}

.participate_tab_color span{
    color:#333333;
}
.participate_tab_color span::after{
    content:'';
    display: block;
    width:1.12rem;
    height:0.06rem;
    background:rgba(255,163,43,1);
    position: relative;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    margin: auto;
}
.human {
    width:100%;
    height:0.72rem;
    background-color: #FFFFFF;
}
.human p {
    float:left;
    margin-left:0.33rem;
    margin-top:0.22rem;
    font-size:0.28rem;
}
.human a {
    float:right;
    border-bottom:0.01rem solid #999999;
    color: #999999;
    font-size:0.24rem;
    margin-right:0.18rem;
    margin-top:0.18rem;
}
.swiper-container{
    position: fixed;
    width:7.5rem;
    top: 3.5rem;
    left: 0;
    right: 0;
    bottom: 0;
}
.participate_main_list,.participate_pick{
    width:7.5rem;
    background: #fff;
    padding-top: 0.36rem;
    padding-bottom: 0.26rem;
    margin-bottom: 1px;
}
.participate_main_con{
    height:1.6rem;
    width:6.9rem;
    background:#F2F2F2 ;
