    color:#333333;
    font-size: 0.24rem;
}
.participate_pick_value small{
    display: block;
    text-align: right;
    color:#333333;
    font-size: 0.28rem;
    font-weight: 600;
    margin-top: 0.18rem;
}
.participate_pick_value small a{
    font-size: 0.24rem;
    font-weight: 400;
}
.participate_pick .tj {
    width:100%;
    height:0.79rem;
    background-color:#FFFFFF;
}
.participate_pick .tj .p1 {
    float:left;
    margin-top:0.28rem;
    margin-left:0.3rem;
    font-size:0.24rem;
    color:#B3B3B3;
    width: 50%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.participate_pick .tj .p2 {
    float:right;
    margin-top:0.28rem;
    margin-right:0.29rem;
    font-size:0.24rem;
    color:#B3B3B3;
}
.sign-list {
    background: #fff;
    margin: 0 0 1px 0;
    padding: 0.2rem 0.3rem;
    position: relative;
}
.sign-list img {
    position: absolute;
    width: 1.8rem;
    height: 1.8rem;
    left: 0.3rem;
    top: 0.2rem;
    display: block;
    border-radius: 0.05rem;
}
.sign-list-cont {
    margin: 0 1.9rem 0 2rem;
    height: 1.8rem;
}
.sign-list-cont h3 {
    height: 0.4rem;
    line-height: 0.4rem;
    overflow : hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}
.sign-list-cont p {
    color: #FF6A22;
    font-size: 0.2rem;
    margin: 0.1rem 0;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    height: 0.4rem;
    line-height: 0.4rem;
}
.sign-list-cont p small {
    font-size: 0.24rem;
    padding: 0 0.2rem 0 0;
}
.sign-list-cont p del {
    font-size: 0.22rem;
    color: #787878;
}
.sign-list-pag {
    height: 0.3rem;
    padding: 0.07rem 0;
    position: relative;
}
.sign-list-child {
    height: 0.16rem;
    margin: 0 1.1rem 0 0;
    background: #FFE5BC;
    border-radius: 0.08rem;
}
.sign-list-child i {
    display: block;
    background:linear-gradient(180deg,rgba(255,176,45,1),rgba(255,106,34,1));
    border-radius: 0.08rem;
    height: 0.16rem;
}
.sign-list-num {
    right: 0;
    top: 0;
    position: absolute;
    height: 0.3rem;
    line-height: 0.3rem;
    width: 1rem;
    color: #FF6A22;
    font-size: 0.22rem;
}
.sign-list-cont span {
    color: #787878;
    font-size: 0.22rem;
}
.mtp {
    padding-top: 0.2rem;
    display: block;
}
.sign-btn {
    width:1.6rem;
    height:0.48rem;
    line-height: 0.48rem;
    text-align: center;
    font-size: 0.26rem;
    font-weight: bold;
    color: #fff;
    background:linear-gradient(90deg,rgba(255,106,34,1),rgba(255,176,45,1));
    border-radius:0.04rem;
    box-shadow:0px 5px 5px 0px rgba(255,106,34,0.4);
    position: absolute;
    right: 0.3rem;
    bottom: 0.4rem;
    display: none;
}
.sign-btn-bj {
    width:1.6rem;
    height:0.48rem;
    line-height: 0.50rem;
    text-align: center;
    font-size: 0.26rem;
    color: #FF7528;    
    border-radius:0.04rem;
    border: 0.02rem solid #FF7528;
    position: absolute;
    right: 0.3rem;
    bottom: 0.4rem;
    display: none;
    background: url(../../image/participate/bj.png) no-repeat 0.03rem center;
    background-size: 0.42rem;
    text-indent: 0.3rem;
}
.pay-success {
	display: none;
	position: fixed;
	width: 100%;
	height: 100%;
	top: 0;
	left: 0;
	z-index: 999;
}
.pay-success-over {
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.5);
}
.pay-success-cont {
	position: absolute;
	width: 5.8rem;
	height: 5rem;
	background: #fff;
	border-radius: 0.18rem;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	margin: auto;
}
.pay-success-cont h3 {
	height: 1rem;
	line-height: 1rem;
	text-align: center;
	font-size: 0.28rem;
	border-bottom: 1px solid #f5f5f5;
	position: relative;
}
.pay-success-cont h3 span {
	width: 0.56rem;
	height: 0.56rem;
	position: absolute;
	right: 0.1rem;
	top: 0.1rem;
	display: block;
	background: url(../../image/popularity/close.png) no-repeat;
	background-size: 100%;
}
.pay-success-cont {
	text-align: center;
}
.pay-success-cont img {
	display: block;
	width: 0.6rem;
	height: 0.6rem;
	margin: 0.2rem auto;
}
.pay-success-cont p {
	height: 0.3rem;
	line-height: 0.3rem;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	font-size: 0.24rem;
}
.pay-success-cont div {
	color: #666;
	font-size: 0.24rem;
	margin: 0.2rem 0.5rem 0.45rem 0.5rem;
}
.pay-success-cont div span:first-child {
	text-align: left;
}
.pay-success-cont div span:last-child {
	text-align: right;
}
.pay-success-cont div:after {
	clear: both;
	content: '';
	display: table;
}
.pay-success-cont small {
	color: #AAAAAA;
	font-size: 0.24rem;
}
.pay-success-cont a {
	display: block;
	height: 0.9rem;
	text-align: center;
	line-height: 0.9rem;
	color: #fff;
	font-size: 0.28rem;
	font-weight: bold;
	background:linear-gradient(90deg,rgba(254,188,80,1),rgba(255,176,45,1),rgba(255,106,34,1));
	border-radius: 0.1rem;
	box-shadow:0px 6px 12px 0px rgba(252,130,57,0.42);
	margin: 0.1rem 0.3rem 0 0.3rem;
}
