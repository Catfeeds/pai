    color:#333333;
    font-weight: 600;
    font-size: 0.26rem;
}
.participate_pick_tit p{
    color:#333333;
    font-size: 0.26rem;
    font-weight: 600;
}
.participate_pick_tit p a{
    font-size: 0.24rem;
    font-weight: 400;
}
.participate_pick_name{
    width: 0.56rem;
    height:0.56rem;
    margin-left: 0.29rem;
    margin-right: 0.08rem;
    overflow: hidden;
    border-radius: 50%;

}
.participate_pick_name img{
    width:100%;
    height:100%;
    display:block;
    /* border-radius: 100%; */
}
.participate_pick_main{
     width:6.9rem;
