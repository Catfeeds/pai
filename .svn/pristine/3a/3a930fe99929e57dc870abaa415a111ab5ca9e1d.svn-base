body{
    background: #fff;
}
.tomoneyresult{
    margin-top: 0.75rem;
}
.tomoneyresult_img{
    width:0.7rem;
    height:0.7rem;
    margin:0 auto;
}
.tomoneyresult_img img{
    width:100%;
    height:100%;
}
.tomoneyresult p{
    text-align: center;
    color:#000;
    font-size: 0.34rem;
    margin-top:0.3rem;
    font-weight: 600;
}
.tomoneyresult span{
    color:#aaaaaa;
    font-size: 0.24rem;
    text-align: center;
    display:block;
    margin-top: 0.2rem;

}
.tomoneyresult_btn{
    width:6.9rem;
    height:0.88rem;
    margin:0 auto;
    margin-top:0.95rem;
    text-align: center;
    font-size: 0.3rem;
    coloe:#000;
    font-weight: 600;
    line-height: 0.88rem;
    box-shadow:0px 18px 21px 0px rgba(255,229,0,0.15);
    border-radius:0.18rem;
    background:linear-gradient(0deg,rgba(255,229,0,1),rgba(255,237,80,1));
}