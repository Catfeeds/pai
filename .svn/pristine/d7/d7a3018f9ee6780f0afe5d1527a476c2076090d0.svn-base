    top:0;
    left: 0;
    z-index: 101;
}
#c{
    margin: 0 auto;
    display: block;
    margin-top: 6px;
}
#r{
    display: block;
    margin: 0 auto;
    opacity: 0;
}
#r::before{
    color: black;
    content: attr(min);
    padding-right: 10px;
}
#r::after{
    color: black;
    content: attr(max);
    padding-left: 10px;
}