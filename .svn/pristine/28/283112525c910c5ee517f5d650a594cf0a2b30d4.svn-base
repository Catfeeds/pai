.fans_empty{
    width:7.5rem;
    margin-top: 1.1rem;
}
.fans_empty img{
    width:2.6rem;
    height:2.6rem;
    margin:0 auto;
    display: block;
    margin-bottom: 0.53rem;

}
.fans_empty p{
    text-align: center;
    font-size: 0.24rem;
    color:#666666;
    margin-top: 0.16rem;
    font-weight: 600;
}
.fans_list{
    background: #fff;
    height:0.99rem;
    line-height: 0.99rem;
    padding-left: 0.3rem;
    padding-right: 0.3rem;
    border-bottom: 1px solid #f2f2f2;
}
.fans_list img{
    width:0.68rem;
    height:0.68rem;

}
.fans_list span{
     font-size: 0.28rem;
    color:#333333;
    margin-left: 0.1rem;
}
.fans_list small{
    float: right;
    color:#666666;
    font-size: 0.24rem;
}