body{
	background: #fff;
}
.success_main{
	width:100%;
	margin:0 auto;
	position: relative;
	border-top: 0.01rem solid #f5f5f5;
}
.success_view{
	margin-left: 1.3rem;
	margin-top: 0.55rem;
}
.apply_img{
	width:1.2rem;
	height:3.6rem;
	float: left;
}
.apply_img img{
	width:100%;
	height:100%;
}
.success_new{
	float:left;
	margin-top: 0.2rem;
	margin-left: 0.2rem;
	/*position: absolute;*/
	/*left: 2.5rem;*/
	/*top:0.5rem;*/
	/*background: url(../../image/register/icon_jd@2x.png) no-repeat center center;*/
}
.success_main .success_first p{
	font-size: 0.32rem;
	color:#000000;
	font-weight: 600;

}
.success_list{
	margin-top:0.5rem;
}
.success_main div p{
	color:#000000;
	font-weight: 600;
	font-size: 0.24rem;
}
.success_main div span{
	font-size: 0.24rem;
	color:#bbbbbb;
}
.success_back{
	width:6.9rem;
	height:0.88rem;
	margin:0 auto;
	margin-top: 0.85rem;
	border-radius: 0.18rem;
	background-color:#FFE500;
	font-size: 0.32rem;
	color:#000000;
	font-weight: 600;
	text-align:center; 
	line-height: 0.88rem;
}
.success_back a{
	display: block;
}
