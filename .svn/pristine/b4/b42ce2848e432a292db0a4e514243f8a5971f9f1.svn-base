}
.index_pop_history p{
    font-size: 0.28rem;
    color:#333333;

}
.index_pop_history div{
    width:0.8rem;
    height:0.36rem;
    background-image: linear-gradient(90deg,
    #FFD400 0%,
    #FFE500 100%),
    linear-gradient(
        #FFD400,
        #FFD400);
    font-size: 0.24rem;
    text-align: center;
    line-height: 0.36rem;
    font-weight: 600;
    border-radius: 0.08rem;
}
.index_pop_history_main{
    margin-top: 0.43rem;
}
.index_pop_history_main div{
    padding:0.15rem 0.26rem;
