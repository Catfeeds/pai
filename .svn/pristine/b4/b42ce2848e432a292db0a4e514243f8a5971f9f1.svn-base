    border:none;
}
.index_pop_text img{
    width:100%;
    height:100%;
}
.index_pop_class{
    width:7.5rem;
    background: #fff;

}
.index_pop_tab{
    width:3.75rem;
    height:1rem;
    text-align: center;
    padding-top: 0.2rem;
}
.index_pop_tab span{
    color:#666666;
    font-size: 0.26rem;
    display: block;
    margin-top: -0.47rem;
}
.index_pop_tab img{
    width:0.76rem;
    height:0.12rem;
    display: block;
    margin:0 auto;
    margin-top:0.4rem;
    opacity: 0;
}
.index_pop_bold span{
    color:#333333;
    font-size: 0.34rem;
    font-weight: 600;
}
.index_pop_bold img{
    opacity: 1;
}
