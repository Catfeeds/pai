    line-height: 0.88rem;
    border-bottom: 0.01rem solid #f5f5f5;
}
.index_pop_search_li span{
    font-size: 0.28rem;
    color:#333333;
}
.index_pop_search_li img{
    width:0.44rem;
    height:0.44rem;
    float:right;
    margin-top: 0.22rem;
    margin-right:0.14rem ;
}
.index_pop_block{
    display: block;
}

.search_fixed{
    position: fixed;
    top:0;
    left: 0;
    z-index: 4;
    background: #fff;
}
.search_index_img{
    width:0.44rem;
    height:0.44rem;
    margin-top: -0.04rem;

}
.search_index_pop{
    position: fixed;
    top:2.79rem;
    left: 0;
    right:0;
    bottom:0;
    display: none;
    z-index: 3;
}
.shop-over {
    background: rgba(0,0,0,0.5);
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
}
.search_index_positon{
    width:7.5rem;
    position: relative;
    background: #fff;
    z-index: 2;
}
.shop_index_tab{
    border-top: 1px solid #f2f2f2;
    display: none;
}
.search_index_price_interval{
    margin: 0.38rem 0.3rem 0 0.67rem;
}
.search_index_price_interval .search_index_text{
    color:#666666;
    font-size: 0.28rem;
    margin-top: 0.09rem;
}
.search_index_inp span{
    margin-left: 0.3rem;
    margin-right: 0.3rem;
    font-size: 0.24rem;
    color:#666666;
}
.search_index_inp input{
    width:1.93rem;
    height:0.58rem;
    background: #f5f5f5;
    border-radius: 0.1rem;
    text-align: center;
    line-height: 0.58rem;
    font-size: 0.24rem;
}
input::-webkit-input-placeholder,textarea::-webkit-input-placeholder{
    color:#b3b3b3;
    font-size: 0.24rem;
}
input:-moz-placeholder,textarea:-moz-placeholder{
    color:#b3b3b3;
    font-size: 0.24rem;
}
input::-moz-placeholder,textarea::-moz-placeholder{
    color:#b3b3b3;
    font-size: 0.24rem;
}
input:-ms-input-placeholder,textarea:-ms-input-placeholder{
    color:#b3b3b3;
    font-size: 0.24rem;
}
.search_index_margin{
    margin: 0.63rem 0.3rem 1rem 0.67rem;
}
.search_index_search{
    margin-top: 0.38rem;
}
.search_index_search div{
    padding:0.1rem 0.2rem;
    border:1px solid #666666;
    border-radius: 0.1rem;
    margin-right: 0.3rem;
}
.search_index_search div p{
    display: inline;
    text-align: center;

}
.search_index_search div p span{
    color:#666666;
    font-size: 0.24rem;
}
.search_index_search div .search_index_img{
    width:0.18rem;
    height:0.13rem;
    display: none;
}
.search_index_search .search_index_red {
    border:1px solid #E54545 ;
}
.search_index_search .search_index_red p{
    color:#E54545;
}
.search_index_search .search_index_red p span{
    color:#E54545;
}
.search_index_search .search_index_red .search_index_img{
    display: inline-block;
}
.search_index_search div .search_index_img img{
    width:100%;
    height:100%;
}
.search_index_status{
    color:#666666;
    font-size:0.24rem;
    padding:0.1rem 0.23rem;
    border:1px solid #666666;
    border-radius: 0.1rem;
    margin-left: 0.54rem;
}
.search_index_btn{
    width:7.5rem;
    height:0.88rem;
    line-height: 0.88rem;
    color:#333333;
    font-size: 0.32rem;
    text-align: center;
    background-image: linear-gradient(0deg,
    #FFD800 0%,
    #FFD500 100%),
    linear-gradient(
        #FFD800,
        #FFD800);
    font-weight: 600;
}
.search_index_status_red{
    color:#E54545;
    border:1px solid #E54545;
}
