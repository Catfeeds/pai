body{
    background: #f5f5f5;
    width:7.5rem;
    /*overflow: hidden!important;*/
}
.orderlist_ul{
    background: #fff;
    height:1rem;
}
.orderlist_ul li{
    float:left;
    width:1.5rem;
    text-align: center;
    line-height: 1rem;
}
.orderlist_ul li span{
    padding-left: 0.1rem;
    padding-right: 0.1rem;
    display: inline-block;
    height:1rem;
}
.orderlist_ul .title_active span{
    border-bottom:0.08rem solid #ffe500;
}
.my_publish_list{
    background: #fff;
    margin-top: 0.2rem
}
.my_publish_tit{
    height:0.72rem;
    background: #fff;
    width:7.5rem;
    line-height: 0.72rem;
}
.my_publish_bh{
    margin-left: 0.3rem;
    color:#000000;
    font-size: 0.24rem;
    font-family:SourceHanSansCN-Regular;
}
.my_publish_bh img{
    width:0.42rem;
    height:0.42rem;
    vertical-align: middle;
    margin-right: 0.16rem;
    object-fit:cover;
}
.my_publish_hint{
    color:#ff4c4c;
    font-size: 0.26rem;
    margin-right: 0.3rem;
}
.ls {
    color: #29DC75
}

.my_publish_green{
    color:#29DC75;
}
.my_publish_main{
    background: #F9F9F9;
    padding-top: 0.32rem;
    padding-bottom: 0.32rem;
}
.my_publish_img{
    width:1.2rem;
    height:1.2rem;
    margin-left: 0.3rem;
    position: relative;
}
.my_publish_img .my_publish_sign{
    width:1.2rem;
    height:0.32rem;
    position: absolute;
    bottom:0;
    left: 0;
}
.my_publish_img .my_publish_sign img{
    width:100%;
    height:100%;
    display: block;
}
.my_publish_img .my_publish_tupain{
    width:100%;
    height:100%;
    object-fit:cover;
}
.my_publish_text{
    width:5.2rem;
    margin-left: 0.3rem;
    margin-right: 0.3rem;
}
.my_publish_text p{
    width:4.3rem;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp: 2;
    overflow: hidden;
    font-size: 0.24rem;
    font-family:ArialMT;
    color:#333333;
    vertical-align: middle;
}
.my_publish_text p img {
    width: 0.86rem;
}
.my_publish_price{
    margin-top: 0.2rem;
    font-size: 0.2rem;
    color:#333333;
}
.my_publish_new{
    color:#333333;
}
.my_publish_new span{
    color:#aaaaaa;
    text-decoration:line-through;
    margin-left: 0.1rem;
}
.my_publish_inventory{
    color:#000000;
}
.my_publish_data{
    height:0.72rem;
    line-height: 0.72rem;
    border-bottom: 0.01rem solid #f2f2f2;
}
.my_publish_data p{
    font-size: 0.24rem;
    color:#000000;
    font-family:ArialMT;
    margin-right: 0.3rem;
    margin-left: 0.3rem;
    font-weight: 600;
}
.my_publish_data p span{
    font-family:SourceHanSansCN-Regular;
}
.my_publish_btn{
    height:0.88rem;
    line-height: 0.88rem;
    padding-left: 0.3rem;
}
.my_publish_share{
    width:1.5rem;
    height:0.46rem;
    border:0.02rem solid #000000;
    text-align: center;
    color:#000000;
    font-size: 0.24rem;
    line-height: 0.46rem;
    border-radius: 0.09rem;
    margin-right: 0.3rem;
    margin-top: 0.2rem;
}

.my_publish_share span{
    color:#000000;
    font-size: 0.24rem;
}
.my_publish_modification{
    width:1.5rem;
    font-weight:600;
    height:0.46rem;
    background: #ffe500;
    border-radius:0.09rem;
    margin-left: 0.2rem;
    margin-top: 0.2rem;
    margin-right: 0.3rem;
    text-align: center;
    line-height: 0.44rem;
    font-size: 0.24rem;
}
.my_publish_modification span{
    font-size: 0.24rem;
    color:#000000;
    font-family:SourceHanSansCN-Regular;
}
.my_publish_lf_text{
    margin-left: 0.31rem;
    font-size: 0.24rem;
}
.my_publish_lf_text span{
    line-height: 0.88rem;
    font-size: 0.24rem;
    font-weight: 600;
}
.my_publish_lf_text p{
    font-weight: 600;
}
.my_publish_black{
    color:#000000;
}
.my_publish_gray{
    color:#aaaaaa;
}
.my_publish_yew{
    color:#F7B830;
}
.my_publish_hui{
    color:#bbbbbb;
}
.my_publish_wid{
    width:1.2rem;
    padding-left: 0.13rem;
    padding-right: 0.13rem;
}
.my_publish_bg{
    background: transparent;
    border:0.01rem solid #333333;
}
.my_pub_share{
    width: 1.6rem;
}
.my_publish_share img{
    width:0.32rem;
    height:0.32rem;
}
/*菜单*/
.top-nav {
    width: 100%;
    height: 0.88rem;
    position: fixed;
    top: 0.89rem;
    left: 0;
    background: #fff;
    
}
.scrollx{
    height: 0.88rem;
    overflow: hidden;
}
.scrollx-content{
    width: 100%;
    overflow-x: auto;
    white-space: nowrap;
}
.top-nav .nav {
    padding: 0 0.1rem;
}
.top-nav .tab-nav li{
    margin:0 0.15rem;
}
.top-nav .nav li{
    display: inline-block;
    vertical-align: middle;
    font-size: 0.26rem;
    color: #AAAAAA;
    height: 0.8rem;
    line-height: 0.88rem;
    padding: 0 0.21rem;
    margin-bottom: 30px;/*把水平滚动条撑到外面,达到隐藏的目的*/
}
.top-nav .wd li {
    padding: 0 0.28rem;
}

.top-nav .nav .active span{
    border-bottom: 3px solid #FFE500;
    color: #000000;
    display:inline-block;
    line-height: 0.8rem;
    padding-left: 0.1rem;
    padding-right:0.1rem ;
}
/*列表*/
.swiper-container{
    position: fixed;
    top: 1.8rem;
    left: 0;
    right: 0;
    bottom: 0;
}
.hs {
    background: #FFE500;
    border: 0.02rem solid #FFE500;
}
.hot {
    border: 0;
    background:linear-gradient(90deg,rgba(255,76,66,1),rgba(255,157,76,1));
    box-shadow:0px 3px 8px 0px rgba(255,119,36,0.3);    
}
.hot span {
    color: #fff;
}
.my_pub_share-hot {
    width: 2.6rem;
    border: 0;
    background:linear-gradient(90deg,rgba(255,76,66,1),rgba(255,157,76,1));
    box-shadow:0px 3px 8px 0px rgba(255,119,36,0.3);
}

.my_pub_share-hot span {
    color: #fff;
}
.layui-layer-btn1 {
    color: #FF6060 !important;
}

.pmg img {
    width: 100%;
    height: 100%;
}
.pmgd {
    width: 5rem;
}
.nav-down {
    width: 50%;
    margin: auto;
    left: 0;
    right: 0;
    top: 0.04rem;
    position: fixed;
    height: 0.8rem;
    background: #fff;
    text-align: center;
    z-index: 99;
    line-height: 0.8rem;
}
.nav-down span {
    color: #000;
    font-size: 0.36rem;
    padding: 0 0.34rem;
    position: relative;
    font-weight: bold;
    background: url(../../image/orderpai/xiala.png) no-repeat center right;
    background-size: 0.24rem;
}
.nav-down .sel {
    background: url(../../image/orderpai/shangla.png) no-repeat center right;
    background-size: 0.24rem;
}
.nav-over {
    position: fixed;
    background: rgba(0, 0, 0, 0.5);
    width: 100%;
    left: 0;
    top: 0;
    z-index: 98;
    display: none;
    height: 100%;
}
.nav-cont {
    display: none;
    position: fixed;
    top: 0.88rem;
    width: 100%;
    left: 0;    
    z-index: 99;
}
.nav-cont a {
    width: 100%;
    height: 0.88rem;
    line-height: 0.88rem;
    display: block;
    background: #fff;
    text-align: center;
    color: #aaa;
    font-size: 0.3rem;
    border-bottom: 1px solid #fafafa;
}
.nav-cont .active {
    color: #000;
}
.mescroll-empty .empty-btn {
    background-color: #FFE500 !important;
    color: #000 !important;
    font-size: 0.26rem !important;
    border: 0 !important;
    width: 1.6rem !important;
    font-weight: bold;
}
.my_publish_renqicolor{
    background:linear-gradient(90deg,rgba(255,76,66,1),rgba(255,157,76,1));
    box-shadow:0px 3px 8px 0px rgba(255,119,36,0.3);
    
}
.my_publish_renqicolor span{
    color:#fff;
}
.hei{
    color:#000000;
}
.xc-icon {
    width: 0.52rem;
    height: 0.51rem;
    position: absolute;
    background: url(../../image/popularity/xc1.png) no-repeat;
    left: 0;
    top: 0;
    z-index: 999;
    background-size: 100%;
}
.cong_xuanzedizhi{
    width:2.4rem;
    height:0.6rem;
    line-height: 0.55rem;
    background:linear-gradient(-90deg,rgba(255,86,67,1),rgba(255,146,75,1));
    border-radius:0.1rem;
    border:none;
    text-align: inherit;
}
.cong_xuanzedizhi img{
    width:0.24rem;
    height:0.3rem;
    margin-left: 0.31rem;
    vertical-align: middle;
}
.cong_xuanzedizhi span{
    margin-left: 0.18rem;
    color:#fff;
    font-size: 0.24rem;
    vertical-align: middle;
}
.fd {
    width: 0.6rem;
    height: 0.37rem;
    background: url(../../image/goodsproduct/icon_11.11biasohi@2x.png) no-repeat;
    background-size: 100%;
    position: absolute;
    top: 0;
    left: 0;
}