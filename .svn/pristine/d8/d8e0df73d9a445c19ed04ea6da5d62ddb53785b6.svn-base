.vip-photo {
	position: relative;
	height: 1.3rem !important;
}
.vip-photo .tx {
	width: 1rem;
	height: 1rem;
	border-radius: 100%;
	border: 2px solid #fff;
	left: 0.4rem;
	top: -0.5rem;
	position: absolute;
	display: block;
	z-index: 9999;
}
.vip-photo h3 {
	margin: 0 3rem 0 1.7rem;
	font-weight: bold;
	font-size: 0.28rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.vip-photo p {
	position: absolute;
	right: 0.3rem;
	top: 0;
	vertical-align: middle;
	background: url(../../image/core/jt.png) no-repeat center right;
	background-size: 0.3rem;
	padding: 0 0.4rem 0 0;
	font-size: 0.22rem;
	font-weight: bold;
}
.vip-photo p span {
	color: #aaa;
	font-weight: normal;
}
.vip-photo p img {
	width: 0.36rem;
	height: 0.36rem;
	border-radius: 100%;
	padding: 0 0.1rem;
}
.open-tj {
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	z-index: 999999;
	display: none;
}
.open-over {
	height: 100%;
	width: 100%;
	background: rgba(0,0,0,0.4);
}
.open-cont {
	width:5.8rem;
	height:3.2rem;
	background:#fff;
	border-radius:0.08rem;
	position: absolute;
	left: 0;
	top: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}
.open-cont span {
	display: block;
	position: absolute;
	width: 0.44rem;
	height: 0.44rem;
	background: url(../../image/core/close.png) no-repeat center;
	background-size: 100%;
	right: 0.57rem;
	top: 0.08rem;
}
.open-cont h3 {
	height: 0.68rem;
	border-bottom: 1px solid #f5f5f5;
	margin: 0 0.9rem 0.26rem 0.9rem;
	text-align: center;
	line-height: 0.68rem;
	color: #666;
	font-weight: bold;
	font-size: 0.24rem;
}
.open-cont img {
	width: 0.68rem;
	height: 0.68rem;
	display: block;
	margin: 0 auto 0.16rem auto;
	border-radius: 100%;
}
.open-cont p {
	color: #333;
	font-size: 0.26rem;
	font-weight: bold;
	text-align: center;
	margin: 0 0 0.34rem 0;
}
.open-cont div {
	margin: 0 1.14rem;
	color: #666;
	font-size: 0.26rem;
}

