#swiper-container3 {
    width: 100%;
    height: 60px;
    position: relative;
    left: 0;
}

#swiper-container3 .swiper-slide {
    height: 0.3rem;
    width: 1.3rem !important;
    font-size: 14px;
    text-align: center;
    color: #000;
}

.timeaxis-current {
    position: absolute;
    top: 0;
    bottom: 0;
    left: 40%;
    right: 40%;
    z-index: 1;
    background: #fc353a;
}

.timeaxis_warp {
    width: 6.6rem;
    height: 0.93rem;
    position: absolute;
    /*background: rgba(43, 43, 43, 1);*/
    border-radius: 0.22rem 0.22rem 0 0;
    top: 0.97rem;
    left: 0.45rem;
}

.timeaxis-item-inner {
    line-height: 20px;
    padding-top: 0.15rem;
}

.timeaxis-time {
    font-size: 0.28rem;
    font-family: PingFangSC-Medium;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
    line-height: 0.3rem;
    opacity: 0.8;
}

.timeaxis-info {
    width: 0.8rem;
    height: 0.2rem;
    margin: 0 auto;
}

.timeaxis-info img {
    width: 100%;
