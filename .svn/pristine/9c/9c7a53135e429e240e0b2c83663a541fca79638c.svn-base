.application_in_box {
    width: 100%;
    background: #fff;
}

.application_in_tit {
    width: 7.5rem;
    height: 0.8rem;
    line-height: 0.8rem;
}

.application_in_text {
    margin-left: 0.31rem;
}

.application_in_tit p {
    font-size: 0.22rem;
    color: rgb(77, 77, 77);
}

.application_in_tit p span {
    color: #FF0000;
    padding-right: 0.1rem;
}

.application_in_list {
    margin-left: 0.3rem;
    margin-right: 0.3rem;
    overflow: hidden;
    border-bottom: 1px solid rgba(245, 245, 245, 1);
    /*position: relative;*/
    padding-top: 0.3rem;
    padding-bottom: 0.3rem;
    overflow: hidden;
}

.application_in_list input {
    width: 5.33rem;
    /*display: block;*/
    float: left;
    font-size: 0.28rem;
    color: #000000;
    margin-left: 0.4rem;
}

.application_in_list p {
    /*position: absolute;*/
    /*left:0;*/
    float: left;
    color: #000000;
    font-weight: bold;
    font-size: 0.28rem;
}

.application_in_list textarea {
    /*display: block;*/
    float: left;
    width: 5.33rem;
    height: 0.44rem;
    margin-left: 0.4rem;
    font-size: 0.28rem;
    color: #000000;
}

.application_in_width {
    padding-left: 0.3rem;
    padding-right: 0.3rem;
    background: rgba(255, 246, 171, 1);
}

.application_in_width p {
    font-size: 0.22rem;
    color: #ffb400;
    padding-top: 0.2rem;
    font-family: SourceHanSansCN-Regular;
    line-height: 0.38rem;
    padding-bottom: 0.2rem;
    font-weight: 600;
}

.application_in_con {
    /*width:80%;*/
    /*margin:0 auto;*/
}

.application_in_license {
    margin: 0 auto;
    margin-top: 0.2rem;
    width: 2.6rem;
    height: 2.6rem;
    margin-bottom: 0.2rem;
    position: relative;
    background-color: rgba(153, 153, 153, 0.498039215686275);
}

.application_in_zhezhao {
    width: 2.6rem;
    height: 2.6rem;
}

.application_in_file {
    position: absolute;
    width: 2.6rem;
    height: 2.6rem;
    top: 0;
    left: 0;
    opacity: 0;
}

.application_in_ify {
    position: relative;
}

.application_in_classify {
    display: block;
    width: 0.15rem;
    height: 0.2rem;
    position: absolute;
    right: 0.2rem;
    top: 0.3rem;
}

.application_in_classify img {
    width: 100%;
    height: 100%;
}

.application_in_agreement {
    margin-bottom: 1rem;
    width: 100%;
    height: 1rem;
    background-color: rgba(245, 245, 245, 1);
    padding-top: 0.2rem;
}

.application_in_agreement .application_div {
    margin-left: 0.3rem;
}

.application_in_agreement input {
    float: left;
    vertical-align: middle;
}

.application_in_agree {
    padding-left: 0.1rem;
    /*float: left;*/
    display: inline-block;
    vertical-align: middle;
    font-size: 0.24rem;
}

.application_in_submit {
    width: 7.5rem;
    height: 1rem;
    position: fixed;
    bottom: 0;
    left: 0;
    background: #dddddd;
    border: none;
    color: #929292;
    font-size: 0.30rem;
}

.input-group .form-control {
    /*输入框里的路径*/
    display: none;
}

.input-group-btn {
    width: 2.6rem;
    height: 2.6rem;
}

.list {
    padding: 0.2rem;
    overflow: hidden;
    position: relative;
}

.file-preview {
    height: 2.6rem;
    width: 2.6rem;
    padding: 0;
}

.form-group {
    width: 2.6rem;
    margin: 0 auto;
}

.file-drop-zone {
    margin: 0;
    padding: 0;
    border: none;
}

.file-drop-zone-title {
    display: none;
}

.krajee-default.file-preview-frame {
    margin: 0;
    padding: 0;
    width: 2.6rem;
    height: 2.6rem;
    margin-right: 0.5rem;
    margin-bottom: 0.2rem;
}

.krajee-default.file-preview-frame .kv-file-content {
    width: 2.6rem;
    height: 2.6rem;
}

.krajee-default .file-footer-caption {
    display: none;
}

.krajee-default .file-upload-indicator {
    display: none;
}

.file-preview-thumbnails {
    display: block;
}

.krajee-default.file-preview-frame .file-thumbnail-footer {
    width: 2.6rem;
    height: 2.6rem;
    border: 1px solid #ddd;
    position: absolute;
    top: 0;
    left: 0;
}

.btn-sm {
    padding: 0;
    width: 0.4rem;
    height: 0.4rem;
}

.kv-file-upload {
    display: none;
}

.kv-file-remove {
    position: absolute;
    right: 0;
    top: 0;
    z-index: 2;
}

.kv-file-zoom {
    width: 1.5rem;
    height: 0.6rem;
    background: rgba(0, 0, 0, 0.5);
    position: absolute;
    left: 0.55rem;
    bottom: 0.31rem;
    border-radius: 0.3rem;
    border: none;
    color: #fff;
    font-size: 0.22rem;
    /*opacity: 0;*/
}

.file-preview .fileinput-remove {
    display: none;
}

.file-preview .fileinput-upload {
    display: none;
}

.file-caption-main {
    width: 2.6rem;
    height: 2.6rem;
    float: left;
    position: absolute;
    top: 0;
    left: 0;
    border: 1px solid #ddd;
    background-color: rgba(153, 153, 153, 0.498039215686275);
    opacity: 0;
}

.btn {
    padding: 0;
}

.btn-primary {
    border: none;
    background-color: rgba(153, 153, 153, 0.498039215686275);
}

.btn-primary:hover {
    border: none;
    background-color: rgba(153, 153, 153, 0.498039215686275);
}

.input-group-btn:last-child>.btn:not(:last-child):not(.dropdown-toggle) {
    display: none;
}

.input-group-btn:last-child>.btn {
    width: 2.6rem;
    height: 2.6rem;
}

.clearfix {
    display: none;
}

.view {
    /*width:2.6rem;*/
    /*margin:0 auto;*/
    margin-top: 0.3rem;
    margin-left: 0.3rem;
    position: relative;
    /*margin-bottom: 0.3rem;*/
    padding-bottom: 0.3rem;
}

.photo {
    width: 2.6rem;
    height: 2.6rem;
    position: relative;
}

.photo img {
    width: 100%;
    height: 100%;
    object-fit:cover;
}

/*.photo p{
			position: absolute;
			bottom:-0.2rem;
			left:0.2rem;
		}*/

.file-preview {
    border: none;
}

.file-preview-image {
    object-fit: cover!important;
    width: 100%!important;
    height: 100%!important;
}

.file-error-message {
    position: fixed;
    top: 42%;
    left: 6%;
}

.application {
    margin-left: 0.3rem;
    margin-right: 0.3rem;
}

.application_deal {
    width: 0.28rem;
    height: 0.28rem;
    vertical-align: middle;
    display: none;
}

.application_block {
    display: inline-block;
}

.application_yew {
    background-image: linear-gradient(0deg, #FFD800 0%, #FFE500 100%), linear-gradient( #FFD800, #FFD800);
    color: #000000;
    font-weight: bold;
}

.hidden-file {
    width: 100%;
    height: 100%;
    cursor: pointer;
    font-size: 30px;
    outline: medium none;
    position: absolute;
    filter: alpha(opacity=0);
    -moz-opacity: 0;
    opacity: 0;
    z-index: 9999;
    display: none;
}


