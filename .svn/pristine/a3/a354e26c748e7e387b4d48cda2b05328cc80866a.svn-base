}

.yzm-view {
    background: #fff;
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: fixed;
    z-index: 100;
    display: none;
}
.yzm-title {
    margin: 1.4rem 0.3rem 0 0.3rem;
    font-size: 0.36rem;
    font-weight: bold;
    color: #333;
}
.yzm-cont {
    font-size: 0.28rem;
    color: #999;
    margin: 0.3rem;
}
.yzm-cont span {
    color: #333;
}
.yzm-input {
    height: 0.8rem;
    border-bottom: 1px solid #EEEEEE;
    position: relative;
    margin: 0 0.3rem;
}
.yzm-input input {
    width: 100%;
    height: 100%;
}
.bangdingcard_code {
    border: 1px solid #333;
    color: #333;
    background: none;
    position: absolute;
    right: 0;
    top: 0.1rem;
    border-radius: 0.3rem;
    padding: 0.1rem 0.2rem;
}
.yzm-btn {
    height:0.88rem;
    background:#FFE500;
    border-radius:0.18rem;
    text-align: center;
    font-size: 0.32rem;
    font-weight: bold;
    line-height: 0.88rem;
    margin: 1rem 0.3rem 0 0.3rem;
}