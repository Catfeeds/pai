.nickname{
	/*width:92%;*/
	/*margin:0 auto;*/
	margin-top: 0.3rem;
	display: none;
	position: fixed;
	top:0;
	left: 0;
	bottom:0;
	right:0;
	background: #f5f5f5;
}
.nickname_view{
	width:100%;
	height:0.88rem;
	margin-top: 0.9rem;
	font-size: 0.26rem;
	text-indent: 0.2rem;
	margin-bottom: 0.3rem;
	background: #fff;
	padding-top: 0.3rem;
	/*line-height: 0.88rem;*/
	position: relative;
}
.nickname_view input{
	background: transparent;
	vertical-align: middle;
	width:6.6rem;
	text-indent: 1rem;
	/*text-align: center;*/
}
.nickname_img{
	width:0.36rem;
	height:0.36rem;
	vertical-align: middle;
	position: absolute;
	top:0.26rem;
	right:0.4rem;
	display: none;
}
.nickname p{
	color:#666666;
	font-size: 0.24rem;
	padding-left: 0.3rem;
	padding-right: 0.3rem;
	font-size: 0.24rem;
	color:#aaaaaa;
	margin-bottom: 0.2rem;
}
.nickname_save{
	width:6.9rem;
	height:0.88rem;
	background-image: linear-gradient(0deg, 
		#FFE500 0%, 
		#FFED50 100%), 
	linear-gradient(
		#FFE500, 
		#FFE500);
	margin:0 auto;
	border-radius: 0.18rem;
	line-height: 0.88rem;
	text-align: center;
	color:#000000;
	margin-top: 0.58rem;
	font-weight: 600;
	font-size: 0.28rem;
}

.sex{
	position: fixed;
	top:0;
	left: 0;
	right:0;
	bottom:0;
	/*background: rgba(0,0,0,0.12);*/
	display: none;
	z-index: 100;
}
.sex-over {
	background: rgba(0,0,0,0.4);
	width: 100%;
	height: 100%;
}
.sex_con{
	width:7.5rem;
	position: absolute;
	bottom:0;
	left: 0;
}
.sex_tit{
	text-align: center;
	padding-top: 0.25rem;
	padding-bottom: 0.25rem;
	color:#aaaaaa;
	font-size: 0.24rem;
	background: #fff;
}
.sex_list{
	text-align: center;
	padding-top: 0.3rem;
	padding-bottom: 0.3rem;
	border-top: 0.01rem solid #f5f5f5;
	color:#000000;
	font-size: 0.3rem;
	background: #fff;
}
.sex_btn{
	text-align: center;
	padding-top: 0.3rem;
	padding-bottom: 0.3rem;
	border-top: 0.01rem solid #f5f5f5;
	color:#000000;
	font-size: 0.3rem;
	background: #fff;
	color:#7E7E7E;
}
.realname{
	position: fixed;
	top:0;
	left: 0;
	right:0;
	bottom:0;
	background: rgba(0,0,0,0.12);
	display: none;
}
.realname_con{
	position: absolute;
	top:3.34rem;
	left: 1.98rem;
	background: #fff;
	width:3.54rem;
	border-radius: 0.12rem;
	box-shadow:18px 0px 21px rgba(0,0,0,0.03);
}
.realname_con p{
	text-align: center;
	padding-top: 0.7rem;
	color:#000000;
	font-size: 0.26rem;
}
.realname_btn{
	margin-left: 0.3rem;
	margin-right:0.3rem;
	margin-top: 0.55rem;
	margin-bottom: 0.3rem;
}
.realname_btn div{
	width:1.32rem;
	height:0.6rem;
	border-radius: 0.08rem;
	background: #ececec;
	text-align: center;
	line-height: 0.6rem;
	font-size: 0.28rem;
	color:#BABABA;
}
.realname_btn .realname_sure{
	background: #FFE500;
	color:#000000;

}
.block{
