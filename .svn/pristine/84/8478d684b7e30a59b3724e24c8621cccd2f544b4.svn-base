.share_list_pulldown{
    position: absolute;
    right:0;
    top:0;
    width:1.96rem;
    height:0.92rem;
    animation: myfirst 0.5s linear 1s infinite alternate;
    /* Safari 与 Chrome: */
    -webkit-animation: myfirst 0.5s linear 1s infinite alternate;
}
.share_list_pulldown img{
    width:100%;
    height:100%;
}
@keyframes myfirst
{
    0%   {top:-0.3rem;}
    100% {top:-0.1rem;}
}
@-moz-keyframes myfirst /* Firefox */
{
    0%   {top:-0.3rem;}
    100% {top:-0.1rem;}
}
@-webkit-keyframes myfirst /* Safari and Chrome */
{
    0%   {top:-0.3rem;}
    100% {top:-0.1rem;}
}
@-o-keyframes myfirst /* Opera */
{
    0%   {top:-0.3rem;}
    100% {top:-0.1rem;}
}
