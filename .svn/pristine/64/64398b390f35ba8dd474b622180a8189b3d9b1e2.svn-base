.pub_margin{
	margin-top: 0.3rem;
	/*border:none;*/
}
.pub_site{
	overflow:hidden;
	/*width: 90%;*/
	position: relative;
	/*line-height: 0.8rem;*/
}
.pub_site input{
	/*float: left;*/
	padding-left: 0.2rem;
	margin-top: 0.01rem;
	text-indent: 0.5rem;
}

.pub_site .pub_dz::-webkit-input-placeholder{
  color:#cccccc;
  font-size: 0.28rem;
}
.pub_site .pub_dz:-moz-placeholder{
  color:#cccccc;
  font-size: 0.28rem;
}
.pub_site .pub_dz::-moz-placeholder{
  color:#cccccc;
  font-size: 0.28rem;
}
.pub_site .pub_dz:-ms-input-placeholder{
  color:#cccccc;
  font-size: 0.28rem;
}
.pub_site .pub_icons{
	float: left;
	width:0.42rem;
	height:0.42rem;
	vertical-align: middle;
	position: absolute;
	top:0.28rem;
	left: 0;
}
.pub_site .pub_icons img{
	width:100%;
	height:100%;
}
.pub_site .pub_icon{
	float: left;
	width:0.28rem;
	height:0.28rem;
	vertical-align: middle;
	position: absolute;
	top:0.35rem;
	left: 0;
	z-index: 2;
	display: none;
}
.pub_site .pub_dis{
	display: block;
}
.pub_site .pub_select{
	width: 0.42rem;
	height:0.42rem;
	position: absolute;
	right:0;
	top:0.23rem;
	/*float: right;*/
	/*margin-top: 0.2rem;*/
}
.pub_site .pub_select img{
	width:100%;
	height:100%;
}
.pub_site .pub_ify{
	text-indent: 2rem;
}
.pub_site p{
	color:#000000;
	width:1.2rem;
	/*float: left;*/
	margin:0;
	text-align: justify;
	text-align-last:justify; 
	position: absolute;
	top:0.28rem;
	font-size: 0.28rem;
	left: 0;
}
.pub_site span{
	float: left;
	margin-left: 0.2rem;
	color:#CACACA;
}
.pub_price input{
	margin-left: 0;
}
.pub_agree_view{
	background: #f5f5f5;
	margin-bottom: 2rem;
	border:none;
	height:0.7rem;
}
/*.pub_agree_input{
	line-height:0;
}*/
.pub_agree_input input{
	width: auto;
	height:auto;
	/*float: left;*/
	vertical-align: middle;
}
.pub_agree{
	padding-left: 0.7rem;
	float: left;
	vertical-align: middle;
}
.pub_submit{
	width: 100%;
	height:1rem;
	background: #DDDDDD;
	position: fixed;
	bottom:0;
	left: 0;
	z-index:2;
	color:#929292;
}
.register_yellow{
	background-image:linear-gradient(top,#FFF,#F2F2F2);
	background-image: linear-gradient(0deg,
	#FFD800 0%,
	#FFE500 100%),
	linear-gradient(
	#FFD800,
	#FFD800);
	color:#000000;
	font-weight: 600;
}
.pub_submit button{
	display: block;
	background: transparent;
	outline: none;
	border: none;
	width: 100%;
	height:100%;
	/*color:#000000;*/
	font-size: 0.3rem;
	/*font-weight: 600;*/
	font-family: SourceHanSansCN;

}
.glyphicon{
	display: none;
}
.kv-file-remove{
	width:0.34rem;
	height:0.34rem;
	background:url(../../icon/publish/icon_deom_del.png) no-repeat center center;
	border:none;
	background-size: 100% 100%;
	border-radius: 0;
}

.pub_choice{
	width:100%;
	height:1.88rem;
	background-color: #fff;
	position: fixed;
	bottom:0;
	left: 0;
	z-index: 33;
	display: none;
}
.pub_choice_top{
	width:90%;
	margin:0 auto;
	height:0.68rem;
	border-bottom:0.01rem solid #ddd;
}
.pub_choice_list{
	width:2.16rem;
	height:0.56rem;
	background-color: #ffe500;
	text-align: center;
	line-height: 0.56rem;
	color:#000000;
	margin-top: 0.2rem;
}
.content_way{
	display: none;
}
.pub_agree_input .pub_check{
	width:0.28rem;
	height:0.28rem;
	position: absolute;
	top:0.35rem;
	left: 0;
	opacity: 0;
}
.pub_explain{
	width:0.4rem;
	height:0.4rem;
	position: absolute;
	top:0.28rem;
	right:0.06rem;
}
.pub_explain img{
	width:100%;
	height:100%;
}
.pub_data input{
	text-indent: 2rem;
}
.photos{
	width:1.58rem;
	height:1.58rem;
	z-index: 5;
	margin-left: 0.24rem;
	margin-top: 0.2rem;
	position: relative;

}
.photos .reedit_old_img{
	width:1.58rem;
	height:1.58rem;
	object-fit: cover;
}
.photos .reedit_cancel_icon{
	position: absolute;
	top:0;
	right:0;
}
.reedit_clear{
	width:0.42rem;
	height:0.42rem;
	position: absolute;
	top:0.28rem;
	right:0;
	display: none;
}
.reedit_clear img{
	width:100%;
	height:100%;
}

.content_number{
	width:7.5rem;
	background: #fafafa;
	padding-bottom: 0.3rem;
	display: none;
}
.content_number_list{
	margin-left:0.37rem;
	margin-right: 0.37rem;
	padding-top: 0.3rem;
	overflow: hidden;
}
.content_number_list img{
	width:0.68rem;
}
.content_number_list span{
	color:#999999;
	font-size: 0.24rem;
}
.content_number_list .content_price_view{
	display: inline-block;
	width:1.9rem;
	margin-left: 0.2rem;
	overflow: hidden;
	vertical-align: middle;
}
.content_number_list .content_margin_left{
	margin-left: 0.37rem;
	
}
.content_number_list .content_number_view{
	display: inline-block;
}
.content_number_list .content_number_view input {
	width: 1.7rem;
	border: 1px solid #000;
	border-radius: 0.05rem;
}
.file-error-message{
	background: rgba(0,0,0,0.5)!important;
	color:#fff!important;
	border:none!important;
}
