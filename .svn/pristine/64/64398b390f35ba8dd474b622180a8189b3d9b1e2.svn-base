}
.input-group .form-control{/*输入框里的路径*/
			display: none;
		}
		.input-group-btn{
			width:1.58rem;
			height:1.58rem;

		}
		.list{
			/*width:90%;*/
			/*margin:0 auto;*/
			/*padding:0.2rem;*/
			overflow:hidden;
			position: relative;
			padding-bottom: 0.2rem;

		}
		.file-preview {
			height:1.7rem;
			padding:0;
		}
		.file-drop-zone{
			margin:0;
			padding:0;
			border:none;
		}
		.file-drop-zone-title{
			display: none;
		}
		.krajee-default.file-preview-frame{
			margin:0;
			padding:0;
			width:1.58rem;
			height:1.58rem;
			margin-left: 0.24rem;
			margin-top: 0.2rem;
			/*margin-bottom: 0.2rem;*/
			box-shadow: none;
		}

		.krajee-default.file-preview-frame .kv-file-content{
			width:1.58rem;
			height:1.58rem;
		}
		.krajee-default.file-preview-frame .kv-file-content img{
			object-fit: cover!important;
			width:100%!important;
			height:100%!important;
		}
		.krajee-default .file-footer-caption{
			display: none;
		}
		.krajee-default .file-upload-indicator {
			display: none;
		}
		.file-preview-thumbnails{
			display: block;
		}
		.krajee-default.file-preview-frame .file-thumbnail-footer{
			width:1.58rem;
			height: 1.58rem;
			position: absolute;
			top:0;
			left: 0;
		}
		.btn-sm {
			padding:0;
			width:0.4rem;
			height:0.4rem;
		}
		.kv-file-upload{
			display: none;
		}
		.kv-file-remove{
			position: absolute;
			right:0;
			top:0;
			z-index:1;
		}
		.kv-file-zoom{
			width:1.58rem;
			height:1.58rem;
			opacity: 0;
		}
		.krajee-default.file-preview-frame{
			border:0;
		}
		.file-preview .fileinput-remove{
			display: none;
		}
		.file-preview .fileinput-upload{
			display: none;
		}
		.file-caption-main{
			width:1.58rem;
			height:1.58rem;
			float: left;
			position: absolute;
			top:0.2rem;
			left: 0.24rem;
			/*border:1px solid #ddd;*/
			opacity: 0;

		}
		.file-input theme-fa{
			float: left;
		}
		.input-group-btn:last-child>.btn:not(:last-child):not(.dropdown-toggle){
			display: none;
		}
		.input-group-btn:last-child>.btn{
			width:1.58rem;
			height:1.58rem;

		}
		.krajee-default.file-preview-frame:first-child{
			/*margin-left: 1.72rem;*/
		}
		/*.krajee-default.file-preview-frame:nth-child(7){
			margin-left: 0rem;
		}
		.krajee-default.file-preview-frame:nth-child(15){
			margin-left: 0rem;
		}*/
		.clearfix{
			display: none;
		}
		.view{
			width:100%;
			background: #fff;
		}
		.photo{
			width:1.58rem;
			height: 1.58rem;
			margin-left: 0.24rem;
			margin-top: 0.2rem;
			/*float: left;*/
			/*position: absolute;*/
			/*top:0rem;*/
			/*left: 0rem;*/
		}
		.photo img{
			width: 100%;
			height:100%;
		}
