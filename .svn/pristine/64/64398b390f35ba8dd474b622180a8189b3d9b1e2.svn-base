body{
	background: #f2f2f2;
}
.content{
	width:100%;
	
	background-color: #fff;
	border-bottom:0.01rem solid #f5f5f5;
}
.pub_list{
	/*width:90%;*/
	margin-left: 0.3rem;
	margin-right: 0.3rem;
	/*margin:0 auto;*/
	/*height:0.88rem;*/
	/*line-height: 0.88rem;*/
	padding-top: 0.3rem;
	padding-bottom: 0.3rem;
	/*height:0.8rem;*/
	position: relative;
}
.pub_list input{
	width:91%;
	/*height:100%;*/
}
.index_textarea{
	height:auto;
	line-height:0.3rem;
}
.pub_list textarea{
	width:100%;
	height:3rem;
	/*padding-top: 0.25rem;*/
