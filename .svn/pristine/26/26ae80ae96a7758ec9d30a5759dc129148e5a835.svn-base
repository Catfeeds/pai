.info_img {
    width: 3.34rem;
    height: 3.34rem;
    /*background: red;*/
    border-radius: 0.16rem 0.16rem 0 0;
}

.content_item_info {
    padding-left: 0.24rem;
    margin-top: 0.24rem;
    /*height:1.5rem;*/
}

.content_info_name {
    width: 2.9rem;
