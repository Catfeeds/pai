body{
   background-color: #FAFAFA;
}
.group_header, .search_top_hearder {
    width: 100%;
    height: 0.88rem;
    background: linear-gradient(270deg, rgba(241, 57, 65, 1) 0%, rgba(252, 68, 107, 1) 100%);
    position: fixed;
    z-index: 3;
    top: 0;
    left: 0;
}

.goback {
    width: 0.22rem;
    height: 0.36rem;
    margin-top: 0.24rem;
    margin-left: 0.48rem;
    margin-right: 0.36rem;
}

.goback img {
    width: 100%;
    height: 100%;
}

.index_search {
    width: 5.4rem;
    height: 0.6rem;
    background: rgba(255, 255, 255, 1);
    opacity: 1;
    border-radius: 0.3rem;
    margin-top: 0.14rem;
}

.index_search img {
    width: 0.24rem;
    height: 0.24rem;
    margin-left: 1.28rem;
    margin-top: 0.18rem;
    margin-right: 0.16rem;
}

.index_search p {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 0.34rem;
    color: rgba(102, 102, 102, 1);
    margin-top: 0.14rem;
}

.group_banner {
    width: 100%;
    height: 2.14rem;
    /*background: url("../../image/activity/top_img@2x.png") center center;*/
    /*background-size: 100%;*/
    margin-top: 0.88rem;
}
.group_banner .swiper-container{
    width: 100%;
    height: 2.14rem;
}
.group_banner .swiper-container .swiper-wrapper{
    width: 100% !important;
    height: 2.14rem!important;
}
.group_banner .swiper-container .swiper-wrapper .swiper-slide{
    width: 100% !important;
    height: 2.14rem!important;
}
.group_banner .swiper-container .swiper-wrapper .swiper-slide img{
    width: 100% !important;
    height: 2.14rem!important;
}
.group_discount_pick {
    background: rgba(252, 66, 73, 1);
    padding-bottom: 0.2rem;
    overflow: hidden;
}

.group_discount {
    padding-top: 0.1rem;
}

.group_discount_item {
    width: 3.6rem;
    height: 2rem;
}

.group_discount_item:nth-child(1) {
    margin-left: 0.1rem;
    margin-right: 0.1rem;
}

.group_discount_item img {
    width: 100%;
    height: 100%;
}

.group_pick {
    margin-top: 0.24rem;
}

.group_pick_top {
    width: 2.91rem;
    height: 0.81rem;
    margin-left: 2.26rem;
}

.group_pick_top img {
    width: 100%;
    height: 100%;
}

.group_pick_content {
    width: 7.5rem;
    height: 3.4rem;
    background: rgba(255, 227, 183, 1);
    border-radius: 0.12rem;
    margin-left: 0.1rem;
}

.swiper-slide {
    width: 1.8rem !important;
}

.pick_item {
    width: 1.8rem;
    padding-top: 0.3rem;
    padding-left: 0.3rem;
    padding-right: 0.6rem;
}

.pick_item_top {
    position: relative;
    margin-left: 0.05rem;
}

.pick_item_top img {
    width: 1.6rem;
    height: 1.6rem;
    background: white;
    border-radius: 0.06rem;
}

.pick_item_top span {
    position: absolute;
    bottom: 0;
    left: 0;
    border-radius: 0 0 0.06rem 0.06rem;
    width: 1.6rem;
    height: 0.4rem;
    background: rgba(0, 0, 0, 0.48);
    text-align: center;
    font-size: 0.22rem;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 0.4rem;
    color: rgba(255, 255, 255, 1);
}

.pick_item_name {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 0.32rem;
    color: rgba(0, 0, 0, 1);
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    /*text-align: center;*/
    margin-top: 0.12rem;
}

.pick_item_price {
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 600;
    line-height: 0.4rem;
    color: rgba(255, 43, 43, 1);
}

.pick_item_price small {
    font-size: 0.2rem;
}

.group_content {
    position: relative;
    background-color: rgba(250,250,250,1);
}

.group_content_top {
    width: 100%;
    height: 0.88rem;
    background-color: white;
    display: none;
}

.top_item {
    width: 25%;
    text-align: center;
    line-height: 0.88rem;
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 400;
    color: rgba(195, 195, 195, 1);
    opacity: 1;
}

.shop_index_tab_default {
    width: 0.18rem;
    height: 0.23rem;
    /*display:none;*/
    display: inline-block;
}

.shop_index_filter {
    width: 0.18rem;
    height: 0.23rem;
    /*display: inline-block;*/
    display: none;
}

.shop_index_filter img {
    width: 100%;
    height: 100%;
    display: none;
}

.shop_index_filter .shop_index_filter_img {
    display: block;
}

.arrow {
    position: relative;
}

.arrow img {
    width: 0.11rem;
    height: 0.11rem;
}

.arrow img:nth-child(1) {
    position: absolute;
    top: 0.1rem;
}

.arrow img:nth-child(2) {

}

.top_item img {
    width: 0.2rem;
    height: 0.18rem;
}

.active {
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 600;
    color: rgba(0, 0, 0, 1);
}

.mescroll {
    margin-top: 0.1rem;
}

.content_item {
    width: 3.34rem;
    background-color: white;
    margin-top: 0.2rem;
    margin-left: 0.3rem;
    /*margin-right: 0.2rem;*/
    padding-bottom: 0.2rem;
    border-radius: 0.16rem;
}

