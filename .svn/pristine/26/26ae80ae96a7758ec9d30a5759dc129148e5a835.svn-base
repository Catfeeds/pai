}

.progress {
    margin-top: 0.06rem;
}

.progress_main {
    width: 2.2rem;
    height: 0.16rem;
    background: rgba(255, 230, 230, 1);
    opacity: 1;
    border-radius: 0.08rem;
    margin-top: 0.1rem;
}

.progress_main span {
    height: 100%;
    display: block;
    border-radius: 0.08rem;
    background: rgba(255, 43, 43, 1);
}

.progress_num {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 500;
    line-height: 0.34rem;
    color: rgba(0, 0, 0, 1);
    opacity: 1;
    margin-left: 0.1rem;
}

.content_info_data {
    margin-top: 0.16rem;
}

.info_price {
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 600;
    line-height: 0.4rem;
    color: rgba(255, 43, 43, 1);
    opacity: 1;
}

.info_price small {
    font-size: 0.2rem;
}

.join {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 0.4rem;
    color: rgba(170, 170, 170, 1);
    opacity: 1;
    margin-right: 0.1rem;
}

.search_condition {
    width: 100%;
    position: absolute;
    top: 0.88rem;
    left: 0;
    background-color: rgba(0,0,0,0.42);
    z-index: 2;
    display: none;
    height: 100%;
}

.condition_item {

}

.condition_item_title {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 400;
    line-height: 0.48rem;
    color: rgba(119, 119, 119, 1);
    opacity: 1;

}

.condition_item_content p {
    width: 2.1rem;
    height: 0.8rem;
    display: inline-block;
    background: rgba(250, 250, 250, 1);
    border-radius: 0.06rem;
    text-align: center;
    line-height: 0.8rem;
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(102, 102, 102, 1);
    margin-top: 0.2rem;
    margin-right: 0.2rem;
}

.input_in {
    font-size: 0.24rem;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(102, 102, 102, 1);
    opacity: 1;
    margin-top: 0.2rem;
}

.input_in input {
    width: 2.9rem;
    height: 0.8rem;
    background: rgba(250, 250, 250, 1);
    border-radius: 0.06rem;
    text-align: center;
    line-height: 0.8rem;

}

input::-webkit-input-placeholder { /* WebKit browsers */
    color: rgba(216, 216, 216, 1);
}

input:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
    color: rgba(216, 216, 216, 1);
}

input::-moz-placeholder { /* Mozilla Firefox 19+ */
    color: rgba(216, 216, 216, 1);
}

input:-ms-input-placeholder { /* Internet Explorer 10+ */
    color: rgba(216, 216, 216, 1);
}

.btns {
    width: 100%;
    /*margin-top: 0.8rem;*/
}

.reset, .affrim {
    width: 50%;
    height: 0.88rem;
    text-align: center;
    line-height: 0.88rem;
}

.reset {
    background: rgba(242, 242, 242, 1);
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(187, 187, 187, 1);
}

.affrim {
    background: rgba(252, 66, 73, 1);
    font-size: 0.28rem;
    font-family: PingFang SC;
    font-weight: 500;
    color: rgba(255, 255, 255, 1);
}

.condition_item_content p.active_in {
    background: rgba(255, 239, 239, 1);
}

