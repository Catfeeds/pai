.taking_empty{
    margin-top: 1.41rem;
}
.taking_empty_img{
    width:1.98rem;
    height:1.98rem;
    margin:0 auto;
    
}
.taking_empty_img img{
    width:100%;
    height:100%;
    display: block;
}
.taking_empty p{
    text-align: center;
    font-size: 0.24rem;
    color:#aaaaaa;
    margin-top: 0.32rem;
}
.taking_list{
    padding:0.24rem 0.35rem ;
    background: #fff;
    border-bottom: 1px solid #f2f2f2;
}
.taking_lf p{
    color:#333333;
    font-size: 0.26rem;
    font-weight: 600;
    padding-bottom: 0.3rem;
}
.taking_rt p{
    color:#080808;
    font-size: 0.36rem;
    font-weight: 600;
    padding-bottom: 0.15rem;
}
.taking_rt span{
    display: block;
    text-align:right;
}
.taking_success{
    font-size: 0.24rem;
    color:#FFCC00;
}
.taking_fail{
    font-size: 0.24rem;
    color:#FE5C5C;
}
.taking_begin{
    font-size: 0.24rem;
    color:#FFCC00;
}
.taking_img{
    width:0.34rem;
    height:0.34rem;
    margin-top: 0.34rem;
    margin-left: 0.5rem;
}
.taking_img img{
    width:100%;
    height:100%;
    display:block;
}
.mescroll-empty{
    margin-top: 1.41rem!important;
}
.mescroll-empty .empty-icon{
    width:1.98rem!important;
    height:1.98rem!important;
    margin-left: 0.28rem!important;
}
.mescroll-empty .empty-tip{
    font-size: 0.24rem!important;
    color:#aaaaaa!important;
    margin-top: 0.32rem!important;
}