.application_popups{
	position:fixed;
	top:0rem;
	left: 0;
	bottom:0;
	right: 0;
	background: #fff;
	z-index: 1000;
	display: none;
}
.glyphicon{
	display: none;
}
.kv-file-remove{
	display: none;
}
.application_menu{
	width:25%;
	background-color: #f8f8f8;
	height:100%;
	min-height:12rem;
	position: fixed;
	top:0rem;
	left: 0;
}
.application_menu li{
	text-align: center;
	height:0.88rem;
	line-height: 0.88rem;
}
.application_menu li a{
	display: block;
	color:#333333;
}
.application_menu li a.on{
	color:#000000;
	border-left: 0.08rem solid #ffe500;
	background-color: #fff;
}
.application_content{
	width:75%;
	margin-left: 25%;
	position: absolute;
	left:5rem;
}

.application_contentDiv{
	padding:0 0.15rem;
	min-height:12rem;
}
.application_tabcon li{
	display: none;
	overflow:hidden;
}
.application_tabcon li a{
	display: block;
	height:0.88rem;
	line-height: 0.88rem;
	margin-left: 0.3rem;
}
.application_tabcon .application_tab_li{
	display: block;
}
.application_dis{
	display: block;
}
#license,#positive,#negative{
	display: block;
	width:2.6rem;
	height:2.6rem;
	position: absolute;
	top:0rem;
	left: 0rem;
}