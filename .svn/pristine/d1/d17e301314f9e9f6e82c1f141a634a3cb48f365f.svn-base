.product_list_nav{
	width:7.5rem;
	height:0.8rem;
	background: #fff;
	line-height: 0.8rem;
	position: relative;
	position: fixed;
	top:0.88rem;
	left: 0;
}
.product_list_lf{
	width:6.7rem;
	overflow: hidden;
	overflow-x:scroll;
}
.product_list_lf span,.product_list_all span{
	/*display:block;*/
	padding-left: 0.3rem;
	text-align: center;
	font-size: 0.26rem;
	color:#B3B3B3;
	/*float: left;*/
}
.product_list_lf .product_list_bold,.product_list_all .product_list_bold,.product_list_sele .product_list_bold{
	font-size: 0.28rem;
	color:#333333;
	font-weight: bold;
}
.product_list_none{
	display: none;
}
.product_list_rt{
	width:0.88rem;
	height: 0.88rem;
	box-shadow:0px 0px 6px rgba(201,201,201,0.24);
	text-align: center;
	position: fixed;
	top: 0.88rem;
	right: 0;
	z-index: 99999;
	background: #fff;
}
.product_list_rt img{
	width:14px;
	display: none;
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	top: 0;
	margin: auto;
}
.product_list_bg{
	position: fixed;
	top:0.88rem;
	left: 0;
	bottom:0;
	right:0;
	display: none;
	z-index:5;
}
.product-over {
	background: rgba(0,0,0,0.5);
	width: 100%;
	height: 100%;
	z-index: -1;
	position: fixed;
	left: 0;
	top:1.86rem;
	display: none;
}
.product_list_sele{
	background: #fff;
	padding-bottom: 0.28rem;
	border-top:1px solid #F5F5F5;
	padding: 0.3rem;
	line-height: 0.6rem;
}
.product_list_sele:after {
	clear: both;
	display: table;
	content: '';
}
.product_list_sele a {
	display: block;
	margin: 0.2rem 0.4rem;
	font-size: 0.26rem;
	float: left;
}
.product_list_rt .product_list_dis{
	display: inline-block;
}
.product_list_title {
	height: 0.88rem;
	line-height: 0.88rem;
	padding: 0 0 0 .5rem;
	color: #333;
	font-size: 0.32rem;
	background: #fff;
	font-weight: bold;
}
.product_list_sele span{
	display: inline-block;
	width:1.81rem;
	margin-bottom: 0.54rem;
	text-align: center;
	color:#666666;
	font-size: 0.26rem;
}
.product_list_sele .product_list_kong{
	display: inline;
	width:0;
	margin:0;

}
.product_list_main{
	width:7.5rem;
	margin-top: 0.89rem;
}
.goods_pro_main{
	margin-top: 0;
}
.product_list_content{
	margin-left: 0.3rem;
	margin-right: 0.3rem;
}
.product_list_list{
	display: inline-block;
	background: #fff;
	width:3.34rem;
	border-radius: 0.08rem;
	padding-bottom: 0.25rem;
	margin-top: 0.2rem;
    margin-right: 0.205rem;
    margin-left: 0.205rem;
}
.goods_pro_main .product_list_content{
	margin:0;
}
.goods_pro_main .product_list_list{
	margin-left: 0.265rem;
	margin-right: 0;
}
.product_list_list:after {
	clear: both;
	content: "";
	display: table;
}
.product_list_pic{
	width:3.34rem;
	height:3.34rem;
	position: relative;
}
.product_list_pic img{
	width:100%;
	height:100%;
	object-fit: contain;
}
.product_list_number{
	padding:0.04rem 0.06rem;
	border-radius:0.088rem 0rem 0rem 0.08rem;
	background: rgba(0,0,0,0.28);
	position: absolute;
	bottom:0;
	right:0;
	color:#fff;
	font-size: 0.24rem;
}
.product_list_list p{
	text-align: left;
	font-size: 0.26rem;
	color:#333333;
	font-weight: bold;
	margin-left: 0.22rem;
	margin-right: 0.22rem;
}
.product_list_list .product_list_tit_p{
	height:0.65rem;
    line-height: 0.35rem;
	overflow : hidden;
	text-overflow: ellipsis;
	display: -webkit-box;
	-webkit-line-clamp: 2;
	-webkit-box-orient: vertical;
	margin-top: 0.15rem;
}
.product_list_list .product_list_oldprice{
	color:#B3B3B3;
	font-size: 0.24rem;
	text-decoration: line-through;
	margin-left: 0.22rem;
	margin-right: 0.22rem;
}
.product_list_hint{
	margin-left: 0.2rem;
	margin-right: 0.2rem;
}
.product_list_hint div{
	border-radius:0.24rem; 
	padding-left:0.11rem;
	padding-right:0.11rem;
	margin-top: 0.16rem;
	margin-bottom: 0.16rem;

}
.product_list_hint .product_list_left{
	border:0.01rem solid #E54545;
	color:#E54545;
}
.product_list_hint .product_list_right{
	border:0.01rem solid #26C97C;
	color:#26C97C;
	margin-left: 0.2rem;
}
.product_list_list .product_list_price{
	margin-top:0.1rem ;
	font-size: 0.24rem;
	color:#E54545;
	height: 0.3rem;
	line-height: 0.3rem;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.product_list_list .product_list_price span{

	color:#b3b3b3;
}
.product_list_list .product_list_price .product_list_red{
	font-size: 0.32rem;
	color:#E54545;
}
.plist-nav {
	position: fixed;
	height: 0.88rem;
	line-height: 0.88rem;
	top: 0.88rem;
	background: #fff;
	width: 100%;
	z-index: 9;
}
.plist {
	margin: 1.86rem 0 0 0;
	width: 7.5rem;
}
.plist-nav-left {
	margin: 0 0.88rem 0 0;
}
.no-content {
	text-align: center;
	padding: 1rem 0;
}
.plist-item {
	display: none;
}
.plist-item:after {
	clear: both;
	content: '';
	display: table;
}
/*菜单*/
.top {
    width: 100%;
    height: 0.88rem;
    position: fixed;
    top: 0.89rem;
    left: 0;
    background: #fff;
}
.top-nav {
    margin: 0 0.8rem 0 0;
}
.scrollx{
    height: 0.88rem;
    overflow: hidden;
}
.scrollx-content{
    width: 100%;
    overflow-x: auto;
    white-space: nowrap;
}
.top-nav .nav {
    padding: 0 0.1rem;
}
.top-nav .nav li {
    display: inline-block;
    vertical-align: middle;
    font-size: 0.26rem;
    color: #B3B3B3;
    height: 0.88rem;
    line-height: 0.88rem;
    padding: 0 0.3rem;
    margin-bottom: 0.3rem;
}
.top-nav .nav .active{
	color: #333333;
	font-size: 0.28rem;
	font-weight: bold;
}
#swiper{
    position: fixed;
    top: 1.86rem;
    left: 0;
    right: 0;
    bottom: 0;
}
.data-list:after {
	clear: both;
	content: '';
	display: table;
}
.swiper-banner {
	width: 100%;
	height: 100%;
}
.swiper-banner .swiper-slide {
	text-align: center;
	/*background: #fff;*/
	/* Center slide text vertically */
	display: -webkit-box;
	display: -ms-flexbox;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-ms-flex-pack: center;
	-webkit-justify-content: center;
	justify-content: center;
	-webkit-box-align: center;
	-ms-flex-align: center;
	-webkit-align-items: center;
	align-items: center;
	width: auto;
	font-size: 0.26rem;
	color: #B3B3B3;
}
.swiper-banner .swiper-wrapper .select {
	color: #333333;
	font-size: 0.28rem;
	font-weight: bold;
}
.clist:after {
    clear: both;
    content: '';
    display: table;
}
.banner img {
	width: 100%;
	margin: 0.88rem 0 0 0
}
.list-tit {
	height: 0.88rem;
	background: url(../../image/index/jx.png) no-repeat center;
	background-size: 1.83rem;
}