body{
    background: #fff;
}
.header_nav{
    background: transparent;
    border-bottom: 0;
}
.header_tit span{
    color:#fff;
}
.rule{
    padding-bottom:1.12rem ;
}
.rule_top{
    width:7.5rem;
    height:3rem;
    background: #201C24;
    position: absolute;
    top:0;
    left: 0;
    z-index:2;
}
.rule_top_text{
    margin-top: 1.7rem;
    margin-left: 0.6rem;
}
.rule_top_text p{
    color:#FFED38;
    font-size: 0.34rem;
    font-weight: 600;
    margin-bottom: 0.15rem;
}
.rule_top_text span{
    color:#B7B7B8;
    font-size: 0.24rem;
}
.rule_top_img{
    width:1.8rem;
    height:1.8rem;
    position: absolute;
    bottom:-0.2rem;
    right:0.82rem;
}
.rule_top_img img{
    width:100%;
    height:100%;
}
.rule_main{
    margin-top:2.5rem;
    padding-bottom: 0.9rem;
}
.rule_list{
    margin-left:0.39rem ;
    height:0.6rem;
    position: relative;
    margin-top: 0.5rem;
}
.rule_list p{
    color:#201C24;
    font-size: 0.28rem;
    font-weight: 600;
    position: absolute;
    top:-0.01rem;
    left: 0.24rem;
}
.rule_list_fudai p{
    top:-0.05rem;
    left: 0.1rem;
}
.rule_bg_img{
    width:0.6rem;
    height:0.6rem;

}
.rule_fudai{
    width:0.3rem;
    height:0.3rem;
}
.rule_bg_img img{
    width:100%;
    height:100%;
}
.rule_bg_juxing{
    width:1.1rem;
    height:0.18rem;
    margin-left: 0.24rem;
}
.rule_bg_juxing img{
    width:100%;
    height:100%;
    /*display: block;*/
}
.rule_main_img{
    width:6rem;
    height:9rem;
}
.rule_main_img img{
    width:100%;
    height:100%;
}
.rule_main_img_fudai{
    width:6.42rem;
    height:6.69rem;
    margin:0 auto;
}
.rule_mian_text{
    margin-left: 0.63rem;
    margin-top: 0.1rem;
    margin-right:0.5rem;
}


.rule_mian_text p{
    color:#201C24;
    font-size: 0.26rem;
    font-weight: 600;
    margin-bottom: 0.05rem;
}
.rule_mian_text span{
    font-size: 0.24rem;
    color:#201C24;
}
.rule_mian_text .rule_main_span{
    color:#666666;
}
.rule_mian_text span a{
    color:#2F4782;
    display: inline;
}
.rule_mian_text span .nodec{
    text-decoration: none;
}
.rule_mian_text a{
    color:#201C24;
    font-size: 0.24rem;
    text-decoration:underline;
    display: block;
    margin-top: 0.32rem;
}
.rule_mian_text .inlinea{
    display: inline;
}
.rule_mian_text small{
    color:#FF2121;
}
.rule_margin{
    margin-top: 0.6rem;
}
.rule_margin_top{
    margin-top: 0.3rem;
}
.gzimg {
    height: auto;
    margin-left: 0.9rem;
    margin-top: 0.5rem;
}
.pointts p {
    color: #FF9F38;
}