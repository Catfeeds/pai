.item {
    float: left;
    height: 0.88rem;
    width: 1.5rem;
    font-size:0.26rem;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(21,21,21,1);
    text-align: center;
    line-height: 0.88rem;
    position: relative;
}

.item.active:after{
    content:'';
    display: block;
    width:0.4rem;
    height:0.12rem;
    background:linear-gradient(90deg,rgba(255,231,68,1),rgba(255,172,28,1));
    border-radius:0.06rem;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}
.content_item{
    border-bottom: 0.01rem solid #f2f2f2;
}
.content_item_detail{
    width: 4.5rem;
    margin-left: 0.3rem;
    margin-top: 0.48rem;
}
.content_item img{
    width:2.4rem;
    height:1.8rem;
    border-radius: 0.18rem;
    background: #0C0C0C;
    margin-right: 0.3rem;
    margin-top: 0.3rem;
    margin-bottom: 0.3rem;
}
.content_item_tit{
    font-size:0.3rem;
    font-family:PingFangSC-Regular;
    font-weight:600;
    color:rgba(0,0,0,1);
    line-height:0.4rem;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
.content_item_data{
    font-size:0.24rem;
    font-family:PingFangSC-Regular;
    font-weight:400;
    color:rgba(170,170,170,1);
    line-height:0.4rem;
    margin-top: 0.45rem;
}

#swiper{
    position: fixed;
    width: 7.5rem;
    top: 8.7rem;
    background-color: white;
}