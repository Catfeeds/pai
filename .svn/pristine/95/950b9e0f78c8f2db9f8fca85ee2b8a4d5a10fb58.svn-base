.circle-progress {
    width: 4.62rem;
    height: 4.62rem;

}
.circle-progress .wrapper {
    width: 50%;
    height: 100%;
    position: absolute;
    top: 0;
    overflow: hidden;
}
.circle-progress .wrapper .circle-progress-bar {
    width: 200%;
    height: 100%;
    box-sizing: border-box;
    border: 12px solid transparent;
    border-radius: 50%;
    position: absolute;
    top: 0;
    transform: rotate(45deg);
}
.circle-progress .wrapper .circle-right {
    border-top: 12px solid white;
    border-right: 12px solid white;
    right: 0;
}
.circle-progress .wrapper .circle-left {
    border-bottom: 12px solid white;
    border-left: 12px solid white;
    left: 0;
}
.circle-progress .wrapper-back {
    width: 100%;
    height: 100%;
    box-sizing: border-box;
    border-radius: 50%;
    border: 12px solid #6C58FF;
}
.circle-progress .right {
    right: 0;
}
.circle-progress .left {
    left: 0;
}
.mask{
    padding-top: 1rem;
    padding-left: 1rem;
}
.mask p{
