body{
    background: #fff;
}
.withdraw_select{
    width:7.5rem;
    padding-top:0.32rem;
    padding-bottom: 0.32rem;

}
.withdraw{
    margin-left:0.59rem;
    margin-right: 0.59rem;
}
.withdraw p{
    color:#000000;
    font-size: 0.28rem;
    font-weight: 600;
}
.withdraw_optfor {
    width: 80%;
}
.withdraw_img{
    width:0.34rem;
    height:0.34rem;
}
.withdraw_img img{
    width:100%;
    height:100%;
}
.withdraw_sum{
    border-top:0.01rem solid #f2f2f2;
}
.withdraw_sum p{
    margin-left: 0.59rem;
    margin-right: 0.59rem;
    color:#000000;
    font-size: 0.24rem;
    font-weight: 600;
}
.withdraw_ipt{
    padding: 0;
}
.withdraw_inp{
    position: relative;
    margin-left:0.59rem;
    margin-right: 0.59rem;
    /*margin-top:0.5rem;*/
    padding-bottom: 0.36rem;
    border-bottom:0.01rem solid #f2f2f2;
}
.withdraw_inp p{
    position: absolute;
    left: 0;
    top:0;
    margin:0;
    font-size: 0.26rem;
    color:#000000;
    font-weight: 600;
}
.withdraw_inp input{
    display: block;
    width:6.3rem;
    margin:0 auto;
    text-indent:0.35rem; ;
}
.withdraw_money{
    padding-top:0.32rem;
    padding-bottom: 0.32rem;
    color:#aaaaaa;
    font-size: 0.24rem;
    padding-left: 0.59rem;
}
.withdraw_money small{
    padding-right:0.59rem ;
}
.withdraw_btn{
    display: block;
    width:6.9rem;
    height:0.88rem;
    background: #dddddd;
    border-radius: 0.18rem;
    line-height: 0.88rem;
    text-align: center;
    font-size: 0.32rem;
    color:#aaaaaa;
    margin:0 auto;
    margin-top: 0.5rem;
}
.withdraw_protocol{
    width:6.9rem;
    margin:0 auto;
    margin-top: 0.57rem;
}
.withdraw_lf_img{
    width:0.28rem;
    height:0.28rem;
    overflow: hidden;
}
.withdraw_lf_img img{
    width:100%;
    height:100%;
    display: none;
}
.withdraw_lf_img .withdraw_img_dis{
    display: block;
}
.withdraw_protocol span{
    font-size: 0.24rem;
    color:#666666;
    margin-left: 0.2rem;
    margin-top: -0.035rem;
}
.withdraw_pop{
    width:7.5rem;
    background: rgba(0,0,0,0.12);
    position:fixed;
    top:0rem;
    bottom:0;
    left: 0;
    right:0;
    z-index:100;
    display: none;
}
.withdraw_bottom{
    position: absolute;
    bottom:0;
    left: 0;
    background: #fff;
    width:7.5rem;
}
.withdraw_tit{
    height:0.72rem;
    line-height: 0.72rem;
    border-bottom: 0.01rem solid #f2f2f2;
}
.withdraw_tit span{
    font-size: 0.24rem;
    color:#666666;
    margin-left:0.31rem ;
}
.withdraw_tit img{
    width:0.34rem;
    height:0.34rem;
    margin-right: 0.46rem;
    margin-top: 0.19rem;
}
.withdraw_pop_list{
    display: none;
    border-bottom: 0.01rem solid #f2f2f2;
}

.withdraw_pop_list_view{
    margin-left: 0.31rem;
    margin-right:0.47rem ;
    padding-top: 0.26rem;
    padding-bottom: 0.26rem;
}
.withdraw_add{
    padding-top: 0.13rem;
    padding-bottom: 0.13rem;
}
.withdraw_add p{
    margin-top: 0.18rem;
}
.withdraw_pop_lf{
    width:0.8rem;
    height:0.8rem;
}
.withdraw_pop_lf img{
    width:100%;
    height:100%;
}
.withdraw_pop_text{
    margin-left: 0.27rem;
}
.withdraw_pop_text p{
    color:#000000;
    font-size: 0.28rem;
    font-weight: 600;
}
.withdraw_pop_text span{
    color:#aaaaaa;
    font-size: 0.24rem;
    font-weight:200;
}
.withdraw_pitch{
    width:0.34rem;
    height:0.34rem;
    margin-top: 0.2rem;
    display: none;
}
.withdraw_pitch img{
    width:100%;
    height:100%;
}
.withdraw_btn_bg{
    background-image: linear-gradient(0deg,
    #FFE500 0%,
    #FFED50 100%),
    linear-gradient(
    #FFE500,
    #FFE500);
    color:#000000;
    font-weight: 600;
}
.ftc_wzsf .srzfmm_box{
    height:auto;
    padding-bottom: 0.35rem;
}
.withdraw_pitch_dis{
    display: block;

}
