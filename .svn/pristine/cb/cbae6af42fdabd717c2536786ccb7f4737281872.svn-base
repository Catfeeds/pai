body{
    background: #f2f2f2;
}
.pay_line{
    width:6.58rem;
    height:1px;
    background: #f2f2f2;
    margin: 0 auto;
    margin-top: 0.06rem;
}
.conf_order_main{
    margin-left: 0.3rem;
    margin-right: 0.3rem;
    padding-top:0;
    padding-bottom: 0;
}
.conf_con_tit{
    padding-top: 0.1rem;
}
.conf_order_text{
    width:auto;
    margin-top: 0.23rem;
}
.conf_order_text p{
    width:4.7rem;
}
.conf_order_img{
    width:1.6rem;
    height:1.6rem;
    margin-left: 0;
}
.conf_order_img img{
    width:100%;
    height:100%;
}
.pay_ranking{
    padding:0.25rem 0.3rem ;
}
.pay_ranking p{
    color:#b3b3b3;
    font-size: 0.24rem;

}
.pay_ranking p span{
    color:#333333;
    font-weight: 600;
    font-size: 0.26rem;

}
.pay_ranking .pay_val{
    margin-right:0.79rem ;
}
.conf_order_new{
    color:#FF622B;
    font-size: 0.24rem;
}
.conf_order_new span{
    font-weight: 400;
}
.payment_success_list{
    margin-top: 0;
}
.pay_result_hint{
    color:#b3b3b3;
    font-size: 0.26rem;
    text-align: center;
    margin-top: 0.4rem;
}
.pay_result_btn{
    width:5.2rem;
    height:0.86rem;
    color:#fff;
    letter-spacing: 0.05rem;
    border-radius: 0.1rem;
    margin-top: 0.21rem;
    background:linear-gradient(-90deg,rgba(255,106,34,1),rgba(255,176,45,1),rgba(254,188,80,1));
}
.pay_result_agin{
    letter-spacing: 0.05rem;
    color:#fff;
    background:linear-gradient(-90deg,rgba(255,106,34,1),rgba(255,176,45,1),rgba(254,188,80,1));
}
